ents":[{"text":"MLNOfflinePack","kind":"identifier"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnofflinepack","title":"MLNOfflinePack","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"text":" ","type":"text"},{"type":"text","text":"a region offline to a local database."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/removePack:withCompletionHandler:":{"type":"topic","url":"\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:","role":"symbol","navigatorTitle":[{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"kind":"symbol","title":"removePack:withCompletionHandler:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:","abstract":[{"type":"text","text":"Unregisters the given offline pack and allows resources that are no longer"},{"type":"text","text":" "},{"type":"text","text":"required by any remaining packs to be potentially freed."}],"fragments":[{"text":"- ","kind":"text"},{"text":"removePack:withCompletionHandler:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","abstract":[{"type":"text","text":"Forcibly, asynchronously reloads the "},{"code":"packs","type":"codeVoice"},{"text":" property. At some point after this","type":"text"},{"type":"text","text":" "},{"text":"method is called, the pointer values of the ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" objects in the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"packs"},{"type":"text","text":" property change, even if the underlying data for these packs has not"},{"type":"text","text":" "},{"text":"changed. If this method is called while a pack is actively downloading, the","type":"text"},{"text":" ","type":"text"},{"text":"behavior is undefined.","type":"text"}],"role":"symbol","title":"reloadPacks","navigatorTitle":[{"kind":"identifier","text":"reloadPacks"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","fragments":[{"kind":"text","text":"- "},{"text":"reloadPacks","kind":"identifier"}]}}}