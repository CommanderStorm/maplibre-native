}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"fragments":[{"text":"MLNOfflinePack","kind":"identifier"}],"kind":"symbol","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference","isActive":true},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"type":"text","text":" "},{"text":"a region offline to a local database.","type":"text"}],"type":"topic","title":"MLNOfflinePack","url":"\/documentation\/maplibre\/mlnofflinepack","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","role":"symbol"}}}