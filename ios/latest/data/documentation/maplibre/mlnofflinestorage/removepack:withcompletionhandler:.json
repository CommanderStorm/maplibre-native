{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:","interfaceLanguage":"occ"},"metadata":{"fragments":[{"kind":"text","text":"- "},{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"roleHeading":"Instance Method","role":"symbol","symbolKind":"method","navigatorTitle":[{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"removePack:withCompletionHandler:","externalID":"c:objc(cs)MLNOfflineStorage(im)removePack:withCompletionHandler:"},"sections":[],"abstract":[{"text":"Unregisters the given offline pack and allows resources that are no longer","type":"text"},{"text":" ","type":"text"},{"text":"required by any remaining packs to be potentially freed.","type":"text"}],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"text":"removePack:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","text":"MLNOfflinePack","preciseIdentifier":"c:objc(cs)MLNOfflinePack"},{"text":" *) ","kind":"text"},{"text":"pack","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"withCompletionHandler:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"void (^)(NSError *)","preciseIdentifier":"c:BFv(#*$objc(cs)NSError)","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"completion","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"parameters":[{"content":[{"inlineContent":[{"text":"The offline pack to remove.","type":"text"}],"type":"paragraph"}],"name":"pack"},{"name":"completion","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The completion handler to call once the pack has been"},{"text":" ","type":"text"},{"text":"removed. This handler is executed asynchronously on the main queue.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"anchor":"discussion","type":"heading","level":2,"text":"Discussion"},{"inlineContent":[{"type":"text","text":"As soon as this method is called on a pack, the pack becomes invalid; any"},{"text":" ","type":"text"},{"type":"text","text":"attempt to send it a message will result in an exception being thrown. If an"},{"type":"text","text":" "},{"type":"text","text":"error occurs and the pack cannot be removed, do not attempt to reuse the pack"},{"type":"text","text":" "},{"text":"object. Instead, if you need continued access to the pack, suspend all packs","type":"text"},{"type":"text","text":" "},{"type":"text","text":"and use the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks","isActive":true,"type":"reference"},{"text":" method to obtain valid pointers to all the packs.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"To detect when any call to this method results in a pack being removed, observe"},{"type":"text","text":" "},{"text":"KVO change notifications on the shared offline storage object’s ","type":"text"},{"code":"packs","type":"codeVoice"},{"type":"text","text":" key"},{"type":"text","text":" "},{"text":"path. Removals from that array result in an ","type":"text"},{"code":"NSKeyValueChangeRemoval","type":"codeVoice"},{"text":" change.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"When you remove an offline pack, any resources that are required by that pack,"},{"text":" ","type":"text"},{"text":"but not other packs, become eligible for deletion from offline storage. Because","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the backing store used for offline storage is also used as a general purpose"},{"text":" ","type":"text"},{"text":"cache for map resources, such resources may not be immediately removed if the","type":"text"},{"type":"text","text":" "},{"text":"implementation determines that they remain useful for general performance of","type":"text"},{"text":" ","type":"text"},{"text":"the map.","type":"text"}]}]}],"kind":"symbol","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/reloadPacks":{"role":"symbol","navigatorTitle":[{"kind":"identifier","text":"reloadPacks"}],"fragments":[{"kind":"text","text":"- "},{"text":"reloadPacks","kind":"identifier"}],"title":"reloadPacks","abstract":[{"text":"Forcibly, asynchronously reloads the ","type":"text"},{"code":"packs","type":"codeVoice"},{"text":" property. At some point after this","type":"text"},{"text":" ","type":"text"},{"text":"method is called, the pointer values of the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","isActive":true},{"type":"text","text":" objects in the"},{"text":" ","type":"text"},{"type":"codeVoice","code":"packs"},{"type":"text","text":" property change, even if the underlying data for these packs has not"},{"text":" ","type":"text"},{"type":"text","text":"changed. If this method is called while a pack is actively downloading, the"},{"text":" ","type":"text"},{"type":"text","text":"behavior is undefined."}],"url":"\/documentation\/maplibre\/mlnofflinestorage\/reloadpacks","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/reloadPacks"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage":{"type":"topic","fragments":[{"text":"MLNOfflineStorage","kind":"identifier"}],"abstract":[{"type":"text","text":"MLNOfflineStorage implements a singleton (shared object) that manages offline"},{"type":"text","text":" "},{"type":"text","text":"packs and ambient caching. All of this class’s instance methods are asynchronous,"},{"type":"text","text":" "},{"type":"text","text":"reflecting the fact that offline resources are stored in a database. The shared"},{"type":"text","text":" "},{"type":"text","text":"object maintains a canonical collection of offline packs in its "},{"code":"packs","type":"codeVoice"},{"text":" property.","type":"text"}],"url":"\/documentation\/maplibre\/mlnofflinestorage","title":"MLNOfflineStorage","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflineStorage/removePack:withCompletionHandler:":{"fragments":[{"kind":"text","text":"- "},{"text":"removePack:withCompletionHandler:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnofflinestorage\/removepack:withcompletionhandler:","type":"topic","abstract":[{"type":"text","text":"Unregisters the given offline pack and allows resources that are no longer"},{"text":" ","type":"text"},{"type":"text","text":"required by any remaining packs to be potentially freed."}],"role":"symbol","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"removePack:withCompletionHandler:"}],"title":"removePack:withCompletionHandler:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflineStorage\/removePack:withCompletionHandler:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"text":" represents a collection of resources necessary for viewing","type":"text"},{"text":" ","type":"text"},{"text":"a region offline to a local database.","type":"text"}],"title":"MLNOfflinePack","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnofflinepack"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","type":"topic","kind":"symbol","title":"MapLibre"}}}