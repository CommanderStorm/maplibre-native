l"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"role":"symbol","title":"MLNOfflinePack","url":"\/documentation\/maplibre\/mlnofflinepack","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"topic","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","type":"reference"},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"type":"text","text":" "},{"text":"a region offline to a local database.","type":"text"}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}]}}}