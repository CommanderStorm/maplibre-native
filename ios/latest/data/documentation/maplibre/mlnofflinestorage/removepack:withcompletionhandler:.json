}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOfflinePack":{"type":"topic","title":"MLNOfflinePack","url":"\/documentation\/maplibre\/mlnofflinepack","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNOfflinePack"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack","role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOfflinePack"},{"type":"text","text":" represents a collection of resources necessary for viewing"},{"type":"text","text":" "},{"text":"a region offline to a local database.","type":"text"}]}}}