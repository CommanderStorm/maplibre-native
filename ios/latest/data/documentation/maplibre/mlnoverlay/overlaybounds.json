title":"MLNOverlay","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}]}}}