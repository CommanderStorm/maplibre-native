ind":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true,"type":"reference"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"type":"text","text":" "},{"text":"take the form of a polyline or polygon.","type":"text"}],"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","title":"MLNOverlay"}}}