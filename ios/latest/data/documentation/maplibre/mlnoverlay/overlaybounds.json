ion\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"]]},"abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"metadata":{"title":"overlayBounds","navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"roleHeading":"Instance Property","symbolKind":"property","fragments":[{"kind":"identifier","text":"overlayBounds"}],"externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","role":"symbol","modules":[{"name":"MapLibre"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","type":"topic","title":"MapLibre","role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"url":"\/documentation\/maplibre\/mlncoordinatebounds","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"fragments":[{"text":"MLNCoordinateBounds","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","type":"topic","title":"MLNCoordinateBounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"title":"overlayBounds","kind":"symbol","fragments":[{"text":"overlayBounds","kind":"identifier"}],"abstract":[{"type":"text","text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","type":"topic","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","fragments":[{"kind":"identifier","text":"MLNOverlay"}],"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference","isActive":true},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"text":"take the form of a polyline or polygon.","type":"text"}],"title":"MLNOverlay","type":"topic","kind":"symbol","role":"symbol"}}}