fragments":[{"text":"MLNOverlay","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnoverlay","type":"topic","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","type":"reference"},{"text":" protocol defines a specific type of annotation that represents","type":"text"},{"text":" ","type":"text"},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}],"role":"symbol","title":"MLNOverlay"}}}