Libre"}],"title":"overlayBounds","roleHeading":"Instance Property","fragments":[{"text":"overlayBounds","kind":"identifier"}],"externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","role":"symbol"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"},"schemaVersion":{"minor":3,"patch":0,"major":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"title":"MLNOverlay","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","kind":"symbol","role":"symbol","type":"topic","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}],"fragments":[{"text":"MLNOverlay","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnoverlay"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"role":"collection","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","role":"symbol"