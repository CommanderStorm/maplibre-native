ontent":[{"type":"text","text":"If this overlay spans the antimeridian, its bounds may extend west of −180 degrees"},{"type":"text","text":" "},{"text":"longitude or east of 180 degrees longitude. For example, an overlay covering the","type":"text"},{"text":" ","type":"text"},{"text":"Pacific Ocean from Tokyo to San Francisco might have a bounds extending","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"from (35.68476, −220.24257) to (37.78428, −122.41310)."}],"type":"paragraph"}]}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"fragments":[{"kind":"identifier","text":"overlayBounds"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","role":"symbol","type":"topic","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"title":"overlayBounds","url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"title":"MLNCoordinateBounds","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCoordinateBounds","role":"symbol","abstract":[{"type":"text","text":"A rectangular area as measured on a two-dimensional map projection."}],"url":"\/documentation\/maplibre\/mlncoordinatebounds","fragments":[{"kind":"identifier","text":"MLNCoordinateBounds"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"url":"\/documentation\/maplibre\/mlnoverlay","kind":"symbol","fragments":[{"kind":"identifier","