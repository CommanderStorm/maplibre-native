":"symbol","sections":[],"metadata":{"role":"symbol","roleHeading":"Instance Property","navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"symbolKind":"property","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds","fragments":[{"text":"overlayBounds","kind":"identifier"}],"title":"overlayBounds"},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds","interfaceLanguage":"occ"},"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"text":"both a point and an area on a map. Overlay objects are essentially data objects","type":"text"},{"text":" ","type":"text"},{"text":"that contain the geographic data needed to represent the map area. Overlays can","type":"text"},{"type":"text","text":" "},{"type":"text","text":"take the form of a polyline or polygon."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","url":"\/documentation\/maplibre\/mlnoverlay","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"kind":"symbol","role":"symbol","title":"MLNOverlay","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","kind":"symbol","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNCoordinateBounds":{"type":"topic","url":"\/documentation\/maplibre\/mlncoordinatebounds","fragments":[{"text":"MLNCoordinateBounds","k