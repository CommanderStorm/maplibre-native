lay\/overlayBounds"},"metadata":{"roleHeading":"Instance Property","role":"symbol","symbolKind":"property","fragments":[{"kind":"identifier","text":"overlayBounds"}],"navigatorTitle":[{"text":"overlayBounds","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"overlayBounds","externalID":"c:objc(pl)MLNOverlay(py)overlayBounds"},"schemaVersion":{"minor":3,"patch":0,"major":0},"kind":"symbol","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","title":"MapLibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay/overlayBounds":{"url":"\/documentation\/maplibre\/mlnoverlay\/overlaybounds","abstract":[{"text":"The cooordinate rectangle that encompasses the overlay. (required) (read-only)","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"overlayBounds"}],"kind":"symbol","type":"topic","title":"overlayBounds","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay\/overlayBounds"},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"role":"symbol","type":"topic","fragments":[{"text":"MLNOverlay","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","kind":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","title":"MLNOverlay","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","isActive":true},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point a