ype":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation"},{"type":"text","text":" object is known as a point annotation or point shape. For"},{"text":" ","type":"text"},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"text":" ","type":"text"},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection/coordinates":{"title":"coordinates","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection\/coordinates","abstract":[{"text":"The array of coordinates associated with the shape.","type":"text"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnpointcollection\/coordinates","kind":"symbol","fragments":[{"kind":"identifier","text":"coordinates"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"url":"\/documentation\/maplibre\/mlnmultipolygonfeature","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates a multipolygon shape with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"fragments":[{"text":"MLNMultiPolygonFeature","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","title":"MLNMultiPolygonFeature","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"url":"\/documentation\/maplibre\/mlnshape","kind":"symbol","abstract":[{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"text":" ","type":"text"},{"type":"text","text":"the content that forms the base map."}],"fragments":[{"text":"MLNShape","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","title":"MLNShape","type":"topic","role":"symbol"}}}