lnpointcollection\/coordinates"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"kind":"symbol","title":"MLNShape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","url":"\/documentation\/maplibre\/mlnshape","type":"topic","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map — not only the overlays atop the map, but also","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the content that forms the base map."}],"fragments":[{"kind":"identifier","text":"MLNShape"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"title":"MLNMultiPolylineFeature","role":"symbol","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","type":"reference","isActive":true},{"text":" object associates a multipolyline shape with an","type":"text"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","type":"topic","fragments":[{"text":"MLNMultiPolylineFeature","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmultipolylinefeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"title":"MLNMultiPolyline","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"type":"text","text":" "},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}],"url":"\/documentation\/maplibre\/mlnmultipolyline","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"}}}