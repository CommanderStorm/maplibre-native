t":"("},{"text":"id<MLNAnnotation>","kind":"typeIdentifier","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"kind":"text","text":") "},{"text":"annotation","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"calloutAccessoryControlTapped:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"UIControl","preciseIdentifier":"c:objc(cs)UIControl"},{"text":" *) ","kind":"text"},{"text":"control","kind":"internalParam"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"mapView","content":[{"inlineContent":[{"text":"The map view containing the specified annotation.","type":"text"}],"type":"paragraph"}]},{"name":"annotation","content":[{"inlineContent":[{"type":"text","text":"The annotation whose accessory view was tapped."}],"type":"paragraph"}]},{"name":"control","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The control that was tapped."}]}]}]},{"content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"In a standard callout view, accessory views contain custom content and are","type":"text"},{"type":"text","text":" "},{"type":"text","text":"positioned on either side of the annotation title text. If an accessory view"},{"text":" ","type":"text"},{"type":"text","text":"you specify is a descendant of the "},{"type":"codeVoice","code":"UIControl"},{"text":" class, the map view calls this","type":"text"},{"text":" ","type":"text"},{"text":"method as a convenience whenever the user taps your view. You can use this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"method to respond to taps and perform any actions associated with that control."},{"type":"text","text":" "},{"text":"For example, if your control displays additional information about the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotation, you could use this method to present a modal panel with that"},{"type":"text","text":" "},{"type":"text","text":"information."}]},{"inlineContent":[{"type":"text","text":"If your custom accessory views are not descendants of the "},{"code":"UIControl","type":"codeVoice"},{"text":" class,","type":"text"},{"text":" ","type":"text"},{"text":"the map view does not call this method. If the annotation has a custom callout","type":"text"},{"type":"text","text":" "},{"type":"text","text":"view via the "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"text":" method, you can specify the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"custom accessory views using the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView"},{"type":"text","text":" protocol’s"},{"text":" ","type":"text"},{"type":"codeVoice","code":"leftAccessoryView"},{"text":" and ","type":"text"},{"code":"rightAccessoryView","type":"codeVoice"},{"type":"text","text":" properties."}],"type":"paragraph"},{"type":"heading","anchor":"Related-examples","text":"Related examples","level":4},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to trigger an action when the"},{"type":"text","text":" "},{"type":"text","text":"standard callout bubble’s accessory control is tapped."}]}],"kind":"content"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNCalloutView":{"fragments":[{"text":"MLNCalloutView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlncalloutview","title":"MLNCalloutView","type":"topic","abstract":[{"text":"A protocol for a ","type":"text"},{"code":"UIView","type":"codeVoice"},{"type":"text","text":" subclass that displays information about a selected"},{"type":"text","text":" "},{"text":"annotation near that annotation.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNCalloutView","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"title":"MLNMapViewDelegate","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"text":" ","type":"text"},{"type":"text","text":"require the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true,"type":"reference"},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"text":" ","type":"text"},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"type":"text","text":" "},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"text":"individual annotations.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","role":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","title":"MapLibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"url":"\/documentation\/maplibre","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"role":"symbol","title":"MLNMapView","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/M