{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"abstract":[{"text":"Returns the view to display on the right side of the standard callout bubble.","type":"text"}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)UIView","text":"UIView"},{"text":" *) ","kind":"text"},{"text":"mapView:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNMapView","text":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" *) ","kind":"text"},{"text":"mapView","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"rightCalloutAccessoryViewForAnnotation:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","kind":"typeIdentifier","text":"id<MLNAnnotation>"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The map view presenting the annotation callout."}]}],"name":"mapView"},{"name":"annotation","content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation with the callout."}],"type":"paragraph"}]}],"kind":"parameters"},{"content":[{"anchor":"return-value","type":"heading","text":"Return Value","level":2},{"type":"paragraph","inlineContent":[{"text":"The accessory view to display.","type":"text"}]}],"kind":"content"},{"content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"inlineContent":[{"type":"text","text":"The right callout view is typically used to convey information about the"},{"type":"text","text":" "},{"text":"annotation or to link to custom information provided by your application.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"If the view you specify is a descendant of the ","type":"text"},{"code":"UIControl","type":"codeVoice"},{"text":" class, you can use","type":"text"},{"type":"text","text":" "},{"text":"the map view’s delegate to receive notifications when your control is tapped,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"by implementing the "},{"code":"-mapView:annotation:calloutAccessoryControlTapped:","type":"codeVoice"},{"text":" ","type":"text"},{"type":"text","text":"method. If the view you specify does not descend from "},{"code":"UIControl","type":"codeVoice"},{"type":"text","text":", your view is"},{"text":" ","type":"text"},{"type":"text","text":"responsible for handling any touch events within its bounds."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"If this method is absent from the delegate, or if it returns ","type":"text"},{"code":"nil","type":"codeVoice"},{"type":"text","text":", the"},{"type":"text","text":" "},{"type":"text","text":"standard callout view has no accessory view on its right side. The return value"},{"text":" ","type":"text"},{"text":"of this method is ignored if ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"text":" is present in","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the delegate."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"To display a view on the callout’s left side, implement the"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"text":" method.","type":"text"}]},{"anchor":"Related-examples","type":"heading","text":"Related examples","level":4},{"inlineContent":[{"type":"text","text":"TODO: Default callout usage, learn how to modify the view that is"},{"type":"text","text":" "},{"type":"text","text":"displayed on the right side of the standard callout bubble."}],"type":"paragraph"}],"kind":"content"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:rightcalloutaccessoryviewforannotation:"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"roleHeading":"Instance Method","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:rightCalloutAccessoryViewForAnnotation:","kind":"identifier"}],"remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L926"},"title":"mapView:rightCalloutAccessoryViewForAnnotation:","role":"symbol","symbolKind":"method","externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:rightCalloutAccessoryViewForAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:rightCalloutAccessoryViewForAnnotation:"}],"modules":[{"name":"MapLibre"}]},"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:rightCalloutAccessoryViewForAnnotation:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:rightCalloutAccessoryViewForAnnotation:":{"abstract":[{"type":"text","text":"Returns the view to display on the right side of the standard callout bubble."}],"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:rightCalloutAccessoryViewForAnnotation:"}],"title":"mapView:rightCalloutAccessoryViewForAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:rightCalloutAccessoryViewForAnnotation:"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:rightcalloutaccessoryviewforannotation:","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:rightCalloutAccessoryViewForAnnotation:","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","abstract":[{"type":"