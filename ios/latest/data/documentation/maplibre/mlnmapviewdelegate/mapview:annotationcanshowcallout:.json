rameters","parameters":[{"name":"mapView","content":[{"inlineContent":[{"type":"text","text":"The map view that has selected the annotation."}],"type":"paragraph"}]},{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The object representing the annotation."}]}]}]},{"content":[{"text":"Return Value","type":"heading","level":2,"anchor":"return-value"},{"inlineContent":[{"type":"text","text":"A Boolean value indicating whether the annotation should show a"},{"text":" ","type":"text"},{"type":"text","text":"callout."}],"type":"paragraph"}],"kind":"content"},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"This method is called after an annotation is selected, before any callout is","type":"text"},{"type":"text","text":" "},{"type":"text","text":"displayed for the annotation."}]},{"inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"YES"},{"text":", a callout view is shown when the user taps on an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotation, selecting it. The default callout displays the annotation’s title"},{"type":"text","text":" "},{"type":"text","text":"and subtitle. You can add accessory views to either end of the callout by"},{"type":"text","text":" "},{"type":"text","text":"implementing the "},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"text":" and","type":"text"},{"text":" ","type":"text"},{"code":"-mapView:rightCalloutAccessoryViewForAnnotation:","type":"codeVoice"},{"type":"text","text":" methods. You can further"},{"type":"text","text":" "},{"text":"customize the callout’s contents by implementing the","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If the return value is "},{"code":"NO","type":"codeVoice"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"type":"text","text":" "},{"type":"text","text":"if the annotation lacks a title, the annotation will not show a callout even"},{"text":" ","type":"text"},{"type":"text","text":"when selected."}]},{"anchor":"Related-examples","type":"heading","text":"Related examples","level":4},{"type":"paragraph","inlineContent":[{"text":"TODO: Add annotation views and images","type":"text"},{"type":"text","text":" "},{"type":"text","text":"TODO: Display custom views as callouts"},{"type":"text","text":" "},{"type":"text","text":"TODO: Default callout usage, learn how to show callouts for"},{"type":"text","text":" "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" objects."}]}],"kind":"content"}],"kind":"symbol","abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"type":"text","text":" "},{"text":"extra information in a callout bubble.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"abstract":[{"type":"text","text":"Returns a Boolean value indicating whether the annotation is able to display"},{"type":"text","text":" "},{"text":"extra information in a callout bubble.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","type":"topic","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"role":"symbol","title":"mapView:annotationCanShowCallout:","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"title":"MLNAnnotation","role":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"text":"provided by Apple’s MapKit.","type":"text"}],"kind":"symbol","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"role":"symbol","type":"topic","title":"MLNMapViewDelegate","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"type":"text","text":" "},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"type":"topic","role":"collection","kind":"symbol"}}}