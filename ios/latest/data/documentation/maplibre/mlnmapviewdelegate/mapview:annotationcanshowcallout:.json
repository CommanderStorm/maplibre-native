ns"},{"kind":"parameters","parameters":[{"name":"mapView","content":[{"inlineContent":[{"text":"The map view that has selected the annotation.","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"The object representing the annotation."}],"type":"paragraph"}],"name":"annotation"}]},{"kind":"content","content":[{"anchor":"return-value","text":"Return Value","level":2,"type":"heading"},{"inlineContent":[{"type":"text","text":"A Boolean value indicating whether the annotation should show a"},{"text":" ","type":"text"},{"text":"callout.","type":"text"}],"type":"paragraph"}]},{"kind":"content","content":[{"level":2,"anchor":"discussion","type":"heading","text":"Discussion"},{"inlineContent":[{"type":"text","text":"This method is called after an annotation is selected, before any callout is"},{"text":" ","type":"text"},{"type":"text","text":"displayed for the annotation."}],"type":"paragraph"},{"inlineContent":[{"text":"If the return value is ","type":"text"},{"code":"YES","type":"codeVoice"},{"type":"text","text":", a callout view is shown when the user taps on an"},{"type":"text","text":" "},{"text":"annotation, selecting it. The default callout displays the annotation’s title","type":"text"},{"type":"text","text":" "},{"type":"text","text":"and subtitle. You can add accessory views to either end of the callout by"},{"type":"text","text":" "},{"text":"implementing the ","type":"text"},{"type":"codeVoice","code":"-mapView:leftCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" and"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:rightCalloutAccessoryViewForAnnotation:"},{"type":"text","text":" methods. You can further"},{"type":"text","text":" "},{"type":"text","text":"customize the callout’s contents by implementing the"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-mapView:calloutViewForAnnotation:"},{"type":"text","text":" method."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If the return value is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":", or if this method is absent from the delegate, or"},{"text":" ","type":"text"},{"text":"if the annotation lacks a title, the annotation will not show a callout even","type":"text"},{"type":"text","text":" "},{"text":"when selected.","type":"text"}],"type":"paragraph"},{"anchor":"Related-examples","text":"Related examples","type":"heading","level":4},{"inlineContent":[{"text":"TODO: Add annotation views and images","type":"text"},{"type":"text","text":" "},{"type":"text","text":"TODO: Display custom views as callouts"},{"type":"text","text":" "},{"type":"text","text":"TODO: Default callout usage, learn how to show callouts for"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" objects."}],"type":"paragraph"}]}],"abstract":[{"text":"Returns a Boolean value indicating whether the annotation is able to display","type":"text"},{"type":"text","text":" "},{"type":"text","text":"extra information in a callout bubble."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:annotationCanShowCallout:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:annotationcanshowcallout:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:annotationCanShowCallout:","abstract":[{"type":"text","text":"Returns a Boolean value indicating whether the annotation is able to display"},{"text":" ","type":"text"},{"text":"extra information in a callout bubble.","type":"text"}],"fragments":[{"kind":"text","text":"- "},{"text":"mapView:annotationCanShowCallout:","kind":"identifier"}],"type":"topic","title":"mapView:annotationCanShowCallout:","navigatorTitle":[{"kind":"identifier","text":"mapView:annotationCanShowCallout:"}],"role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","kind":"symbol","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","kind":"symbol","fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","type":"reference"},{"text":" protocol defines a set of optional methods that you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"text":"require the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"type":"text","text":" "},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"type":"text","text":" "},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"individual annotations."}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","title":"MapLibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","role":"collection"}}}