e":"heading","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Implement this method to mark a point annotation with a static image. If you","type":"text"},{"type":"text","text":" "},{"text":"want to mark a particular point annotation with an annotation view instead,","type":"text"},{"text":" ","type":"text"},{"text":"omit this method or have it return ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" for that annotation, then implement"},{"type":"text","text":" "},{"code":"-mapView:viewForAnnotation:","type":"codeVoice"},{"text":".","type":"text"}]},{"type":"paragraph","inlineContent":[{"text":"Static annotation images use less memory and draw more quickly than annotation","type":"text"},{"type":"text","text":" "},{"type":"text","text":"views. On the other hand, annotation views are compatible with UIKit, Core"},{"type":"text","text":" "},{"text":"Animation, and other Cocoa Touch frameworks.","type":"text"}]},{"type":"heading","text":"Related examples","level":4,"anchor":"Related-examples"},{"inlineContent":[{"type":"text","text":"TODO: Annotation models"},{"text":" ","type":"text"},{"type":"text","text":"TODO: Add annotation views and images"},{"text":" ","type":"text"},{"type":"text","text":"TODO: Mark a place on the map with an image, learn to specify which"},{"type":"text","text":" "},{"type":"text","text":"image should be used for "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" objects that have been added to"},{"type":"text","text":" "},{"text":"your map","type":"text"}],"type":"paragraph"}],"kind":"content"}],"schemaVersion":{"major":0,"minor":3,"patch":0},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:imageforannotation:"]}],"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:imageForAnnotation:","interfaceLanguage":"occ"},"metadata":{"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"mapView:imageForAnnotation:"}],"symbolKind":"method","roleHeading":"Instance Method","navigatorTitle":[{"text":"mapView:imageForAnnotation:","kind":"identifier"}],"role":"symbol","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L460","fileName":"MLNMapViewDelegate.h"},"modules":[{"name":"MapLibre"}],"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:imageForAnnotation:","title":"mapView:imageForAnnotation:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"kind":"symbol","type":"topic","title":"MLNAnnotationImage","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference","isActive":true},{"text":" class is responsible for presenting point-based","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"code":"UIImage","type":"codeVoice"},{"type":"text","text":" "},{"text":"objects and may be recycled later and put into a reuse queue that is maintained","type":"text"},{"type":"text","text":" "},{"type":"text","text":"by the map view."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:imageForAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:imageForAnnotation:","role":"symbol","abstract":[{"text":"Returns an annotation image object to mark the given point annotation object on","type":"text"},{"text":" ","type":"text"},{"text":"the map.","type":"text"}],"title":"mapView:imageForAnnotation:",