,{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"text":" ","type":"text"},{"text":"individual annotations.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"kind":"symbol","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"role":"symbol","title":"MLNAnnotationImage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"text":" class is responsible for presenting point-based","type":"text"},{"type":"text","text":" "},{"text":"annotations visually on a map view. Annotation image objects wrap ","type":"text"},{"code":"UIImage","type":"codeVoice"},{"type":"text","text":" "},{"text":"objects and may be recycled later and put into a reuse queue that is maintained","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"by the map view."}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotationimage"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"fragments":[{"kind":"identifier","text":"MLNMapView"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","url":"\/documentation\/maplibre","title