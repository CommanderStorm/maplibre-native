{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"]]},"abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"metadata":{"modules":[{"name":"MapLibre"}],"externalID":"c:objc(pl)MLNMapViewDelegate(im)mapView:viewForAnnotation:","fragments":[{"text":"- ","kind":"text"},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"role":"symbol","remoteSource":{"fileName":"MLNMapViewDelegate.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNMapViewDelegate.h#L716"},"symbolKind":"method","title":"mapView:viewForAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"roleHeading":"Instance Method"},"schemaVersion":{"minor":3,"patch":0,"major":0},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:"]}],"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","interfaceLanguage":"occ"},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"tokens":[{"kind":"text","text":"- ("},{"text":"MLNAnnotationView","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"kind":"text","text":" *) "},{"kind":"identifier","text":"mapView:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","preciseIdentifier":"c:objc(cs)MLNMapView"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"mapView"},{"text":" ","kind":"text"},{"kind":"identifier","text":"viewForAnnotation:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseId