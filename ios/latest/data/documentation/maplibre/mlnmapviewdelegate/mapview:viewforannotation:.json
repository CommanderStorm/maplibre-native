on":{"url":"\/documentation\/maplibre\/mlnannotation","type":"topic","title":"MLNAnnotation","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},{"type":"text","text":" protocol defines a set of optional methods that you"},{"text":" ","type":"text"},{"text":"can use to receive map-related update messages. Because many map operations","type":"text"},{"type":"text","text":" "},{"type":"text","text":"require the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"reference","isActive":true},{"text":" class to load data asynchronously, the map view calls","type":"text"},{"type":"text","text":" "},{"text":"these methods to notify your application when specific operations complete. The","type":"text"},{"text":" ","type":"text"},{"text":"map view also uses these methods to request information about annotations","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"displayed on the map, such as the styles and interaction modes to apply to"},{"type":"text","text":" "},{"text":"individual annotations.","type":"text"}],"kind":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapviewdelegate","fragments":[{"kind":"identifier","text":"MLNMapViewDelegate"}],"title":"MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"abstract":[{"text":"The MLNUserLocation class defines a specific type of annotation that identifies","type":"text"},{"type":"text","text":" "},{"text":"the user’s current location. You do not create instances of this class","type":"text"},{"type":"text","text":" "},{"text":"directly. Instead, you retrieve an existing ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","isActive":true,"type":"reference"},{"type":"text","text":" object from the"},{"type":"text","text":" "},{"type":"codeVoice","code":"userLocation"},{"type":"text","text":" property of the map view displayed in your application."}],"fragments":[{"kind":"identifier","text":"MLNUserLocation"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnuserlocation","title":"MLNUserLocation","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"type":"topic","title":"MLNUserLocationAnnotationView","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNUserLocationAnnotationView"}],"url":"\/documentation\/maplibre\/mlnuserlocationannotationview","abstract":[{"text":"View representing an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"text":" on screen.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"kind":"symbol","title":"MLNAnnotationView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview","role":"symbol","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol. When an annotation’s","type":"text"},{"type":"text","text":" "},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"type":"topic","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"title":"mapView:viewForAnnotation:","ur