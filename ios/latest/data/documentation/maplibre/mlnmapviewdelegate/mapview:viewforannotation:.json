elegate/mapView:viewForAnnotation:":{"title":"mapView:viewForAnnotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}],"type":"topic","abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"mapView:viewForAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","title":"MapLibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"type":"topic","role":"collection","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"title":"MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"text":" ","type":"text"},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocation":{"title":"MLNUserLocation","type":"topic","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnuserlocation","abstract":[{"type":"text","text":"The MLNUserLocation class defines a specific type of annotation that identifies"},{"type":"text","text":" "},{"type":"text","text":"the user’s current location. You do not create instances of this class"},{"text":" ","type":"text"},{"type":"text","text":"directly. Instead, you retrieve an existing "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"text":" object from the","type":"text"},{"type":"text","text":" "},{"code":"userLocation","type":"codeVoice"},{"text":" property of the map view displayed in your application.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation","fragments":[{"kind":"identifier","text":"MLNUserLocation"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate":{"fragments":[{"text":"MLNMapViewDelegate","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapviewdelegate","title":"MLNMapViewDelegate","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate","isActive":true},{"type":"text","text":" protocol defines a set of optional methods that you"},{"type":"text","text":" "},{"type":"text","text":"can use to receive map-related update messages. Because many map operations"},{"type":"text","text":" "},{"text":"require the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","isActive":true},{"type":"text","text":" class to load data asynchronously, the map view calls"},{"text":" ","type":"text"},{"type":"text","text":"these methods to notify your application when specific operations complete. The"},{"type":"text","text":" "},{"type":"text","text":"map view also uses these methods to request information about annotations"},{"text":" ","type":"text"},{"text":"displayed on the map, such as the styles and interaction modes to apply to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"individual annotations."}],"role":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"title":"MLNAnnotation","type":"topic","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserLocationAnnotationView":{"kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocationAnnotationView","abstract":[{"text":"View representing an ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserLocation"},{"text":" on screen.","type":"text"}],"fragments":[{"text":"MLNUserLocationAnnotationView","kind":"identifier"}],"title":"MLNUserLocationAnnotationView","url":"\/documentation\/maplibre\/mlnuserlocationannotationview","role":"symbol"}}}