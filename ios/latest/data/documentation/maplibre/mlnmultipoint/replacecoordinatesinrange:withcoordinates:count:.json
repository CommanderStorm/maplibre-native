text":"If "},{"code":"count","type":"codeVoice"},{"text":" is greater than the ","type":"text"},{"type":"codeVoice","code":"length"},{"text":" field of ","type":"text"},{"type":"codeVoice","code":"range"},{"type":"text","text":", some vertices will"},{"text":" ","type":"text"},{"type":"text","text":"effectively be inserted into the shape. On the other hand, if "},{"code":"count","type":"codeVoice"},{"text":" is less","type":"text"},{"text":" ","type":"text"},{"text":"than the ","type":"text"},{"code":"length","type":"codeVoice"},{"text":" field of ","type":"text"},{"type":"codeVoice","code":"range"},{"text":", some vertices will effectively be removed.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If "},{"type":"codeVoice","code":"range"},{"text":" extends beyond the shape’s ","type":"text"},{"code":"coordinates","type":"codeVoice"},{"text":" property, an","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"NSRangeException"},{"text":" is raised. If you want to append new vertices to the shape,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"use the "},{"type":"codeVoice","code":"-appendCoordinates:count:"},{"text":" method.","type":"text"}]}],"kind":"content"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmultipoint\/replacecoordinatesinrange:withcoordinates:count:"],"traits":[{"interfaceLanguage":"occ"}]}],"metadata":{"title":"replaceCoordinatesInRange:withCoordinates:count:","navigatorTitle":[{"kind":"identifier","text":"replaceCoordinatesInRange:withCoordinates:count:"}],"roleHeading":"Instance Method","role":"symbol","modules":[{"name":"MapLibre"}],"symbolKind":"method","externalID":"c:objc(cs)MLNMultiPoint(im)replaceCoordinatesInRange:withCoordinates:count:","fragments":[{"text":"- ","kind":"text"},{"text":"replaceCoordinatesInRange:withCoordinates:count:","kind":"identifier"}]},"kind":"symbol","schemaVersion":{"patch":0,"minor":3,"major":0},"abstract":[{"type":"text","te