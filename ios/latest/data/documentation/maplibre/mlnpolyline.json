rchard.","type":"text"}],"title":"MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","type":"topic","fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","kind":"symbol","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:strokeColorForShapeAnnotation:":{"fragments":[{"text":"- ","kind":"text"},{"text":"mapView:strokeColorForShapeAnnotation:","kind":"identifier"}],"title":"mapView:strokeColorForShapeAnnotation:","role":"symbol","url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:strokecolorforshapeannotation:","navigatorTitle":[{"kind":"identifier","text":"mapView:strokeColorForShapeAnnotation:"}],"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:strokeColorForShapeAnnotation:","abstract":[{"type":"text","text":"Returns the color to use when rendering the outline of a shape annotation."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNOverlay":{"fragments":[{"kind":"identifier","text":"MLNOverlay"}],"title":"MLNOverlay","role":"symbol","url":"\/documentation\/maplibre\/mlnoverlay","type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay","abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNOverlay"},{"type":"text","text":" protocol defines a specific type of annotation that represents"},{"type":"text","text":" "},{"type":"text","text":"both a point and an area on a map. Overlay objects are essentially data objects"},{"type":"text","text":" "},{"type":"text","text":"that contain the geographic data needed to represent the map area. Overlays can"},{"text":" ","type":"text"},{"type":"text","text":"take the form of a polyline or polygon."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolylineFeature":{"type":"topic","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature"},{"text":" object associates a multipolyline shape with an","type":"text"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"title":"MLNMultiPolylineFeature","url":"\/documentation\/maplibre\/mlnmultipolylinefeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolylineFeature","fragments":[{"kind":"identifier","text":"MLNMultiPolylineFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"type":"topic","role":"symbol","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","isActive":true},{"text":" object associates an empty shape with an optional","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"identifier and attributes."}],"title":"MLNEmptyFeature","url":"\/documentation\/maplibre\/mlnemptyfeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","fragments":[{"kind":"identifier","text":"MLNEmptyFeature"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipoint","role":"symbol","type":"topic","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference","isActive":true},{"type":"text","text":" class is an abstract superclass used to define shapes"},{"text":" ","type":"text"},{"type":"text","text":"composed of multiple vertices."}]}}}