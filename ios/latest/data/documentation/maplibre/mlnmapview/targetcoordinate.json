t"},{"type":"text","text":"transition. If you don’t want to animate the change, use the"},{"text":" ","type":"text"},{"type":"codeVoice","code":"-setTargetCoordinate:animated:"},{"type":"text","text":" method instead."}]}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate","interfaceLanguage":"occ"},"metadata":{"symbolKind":"property","title":"targetCoordinate","fragments":[{"kind":"identifier","text":"targetCoordinate"}],"role":"symbol","modules":[{"name":"MapLibre"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","roleHeading":"Instance Property","navigatorTitle":[{"kind":"identifier","text":"targetCoordinate"}]},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"text":"provided by Apple’s MapKit.","type":"text"}],"kind":"symbol","role":"symbol","title":"MLNMapView","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","fragments":[{"text":"MLNMapView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNUserTrackingMode/MLNUserTrackingModeFollowWithCourse":{"fragments":[{"text":"MLNUserTrackingModeFollowWithCourse","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLib