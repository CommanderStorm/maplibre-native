t"},{"type":"text","text":"transition. If you don’t want to animate the change, use the"},{"type":"text","text":" "},{"type":"codeVoice","code":"-setTargetCoordinate:animated:"},{"type":"text","text":" method instead."}]}]}],"metadata":{"symbolKind":"property","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"targetCoordinate"}],"role":"symbol","title":"targetCoordinate","fragments":[{"kind":"identifier","text":"targetCoordinate"}],"externalID":"c:objc(cs)MLNMapView(py)targetCoordinate","roleHeading":"Instance Property"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/targetCoordinate"},"schemaVersion":{"patch":0,"major":0,"minor":3},"abstract":[{"type":"text","text":"The geographic coordinate that is the subject of observation as the user"},{"type":"text","text":" "},{"type":"text","text":"location is being tracked."}],"sections":[],"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/targetcoordinate"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNUserTrackingMode/MLNUserTrackingModeFollowWithCourse":{"type":"topic","fragments":[{"kind":"identifier","text":"MLNUserTrackingModeFollowWithCourse"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnusertrackingmode\/mlnusertrackingmodefollowwithcourse","role":"symbol","abstract":[],"title":"MLNUserTrackingModeFollowWithCourse","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNUserTrackingMode\/MLNUserTrackingModeFollowWithCourse"},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"role":"collection","url":"\/documentation\/maplibre","kind