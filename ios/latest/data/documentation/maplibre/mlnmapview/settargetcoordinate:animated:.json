{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"metadata":{"symbolKind":"method","externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"role":"symbol","roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"title":"setTargetCoordinate:animated:","navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}]},"abstract":[{"type":"text","text":"Deprecated. Sets the geographic coordinate that is the subject of observation as"},{"type":"text","text":" "},{"text":"the user location is being tracked, with an optional transition animation.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/settargetcoordinate:animated:"]}],"sections":[],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"kind":"text","text":") "},{"text":"setTargetCoordinate:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"targetCoordinate"},{"text":" ","kind":"text"},{"text":"animated:","kind":"identifier"},{"kind":"text","text":"("},{"text":"BOOL","kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL"},{"text":") ","kind":"text"},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}]},{"kind":"parameters","parameters":[{"name":"targetCoordinate","content":[{"inlineContent":[{"type":"text","text":"The target coordinate to fit within the viewport."}],"type":"paragraph"}]},{"name":"animated","content":[{"inlineContent":[{"text":"If ","type":"