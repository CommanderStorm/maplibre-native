{"metadata":{"title":"setTargetCoordinate:animated:","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","roleHeading":"Instance Method","symbolKind":"method","modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"text":"setTargetCoordinate:animated:","kind":"identifier"}]},"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:","interfaceLanguage":"occ"},"abstract":[{"text":"Deprecated. Sets the geographic coordinate that is the subject of observation as","type":"text"},{"type":"text","text":" "},{"text":"the user location is being tracked, with an optional transition animation.","type":"text"}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"kind":"text","text":") "},{"text":"setTargetCoordinate:","kind":"identifier"},{"kind":"text","text":"("},{"text":"CLLocationCoordinate2D","preciseIdentifier":"c:@S@CLLocationCoordinate2D","kind":"typeIdentifier"},{"kind":"text","text":") "},{"kind":"internalParam","text":"targetCoordinate"},{"kind":"text","text":" "},{"text":"animated:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"BOOL","preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"parameters":[{"name":"targetCoordinate","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The target coordinate to fit within the viewport."}]}]},{"name":"animated","content":[{"inlineContent":[{"text":"If ","type":"text"},{"type":"codeVoice","code":"YES"},{"text":", the map animates to fit the target within the map","type":