{"metadata":{"roleHeading":"Instance Method","externalID":"c:objc(cs)MLNMapView(im)setTargetCoordinate:animated:","modules":[{"name":"MapLibre"}],"symbolKind":"method","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"setTargetCoordinate:animated:"}],"navigatorTitle":[{"text":"setTargetCoordinate:animated:","kind":"identifier"}],"title":"setTargetCoordinate:animated:","role":"symbol"},"abstract":[{"text":"Deprecated. Sets the geographic coordinate that is the subject of observation as","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the user location is being tracked, with an optional transition animation."}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/setTargetCoordinate:animated:"},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","text":"void","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"setTargetCoordinate:","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@S@CLLocationCoordinate2D","text":"CLLocationCoordinate2D","kind":"typeIdentifier"},{"text":") ","kind":"text"},{"text":"targetCoordinate","kind":"internalParam"},{"text":" ","kind":"text"},{"kind":"identifier","text":"animated:"},{"kind":"text","text":"("},{"text":"BOOL","kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL"},{"kind":"text","text":") "},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}]}]},{"kind":"parameters","parameters":[{"name":"targetCoordinate","content":[{"inlineContent":[{"text":"The target coordinate to fit within the viewport.","type":"text"}],"type":"paragraph"}]},{"name":"ani