ion\/MapLibre\/MLNMultiPoint"},{"type":"text","text":" "},{"text":"objects that are not instances of ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"text":". Any","type":"text"},{"type":"text","text":" "},{"type":"text","text":"multipoint, multipolyline, multipolygon, or shape collection objects that"},{"text":" ","type":"text"},{"text":"are specified are silently ignored.","type":"text"}]}]}],"kind":"content"}],"sections":[],"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/addannotations:"]}],"metadata":{"symbolKind":"method","roleHeading":"Instance Method","role":"symbol","navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"title":"addAnnotations:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addAnnotations:"}],"externalID":"c:objc(cs)MLNMapView(im)addAnnotations:"},"schemaVersion":{"major":0,"minor":3,"patch":0},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"title":"MLNMultiPoint","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference","isActive":true},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"type":"text","text":" "},{"type":"text","text":"composed of multiple vertices."}],"url"