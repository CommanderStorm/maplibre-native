pe":"paragraph","inlineContent":[{"text":"","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"type":"text","text":", "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"type":"text","text":", and "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"text":" objects","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"cannot be added to the map view at this time. Nor can "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","type":"reference","isActive":true},{"type":"text","text":" "},{"type":"text","text":"objects that are not instances of "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"type":"text","text":". Any"},{"text":" ","type":"text"},{"text":"multipoint, multipolyline, multipolygon, or shape collection objects that","type":"text"},{"text":" ","type":"text"},{"text":"are specified are silently ignored.","type":"text"}]}],"type":"aside","name":"Note","style":"note"}]}],"abstract":[{"text":"Adds an array of annotations to the map view.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","role":"symbol","title":"MLNMultiPolyline","type":"topic","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"text":" ","type":"text"},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"text":" object.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolyline","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"kind":"symbol","role":"collection","url":"\/documentation\/maplibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","role":"symbol","title":"MLNPolyline","type":"topic","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"type":"text","text":" "},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"text":" ","type":"text"},{"type":"text","text":"path along which something moves."}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnpolyline","fragments":[{"text":"MLNPolyline","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPoint":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","url":"\/documentation\/maplibre\/mlnmultipoint","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPoint","isActive":true},{"text":" class is an abstract superclass used to define shapes","type":"text"},{"type":"text","text":" "},{"text":"composed of multiple vertices.","type":"text"}],"role":"symbol","fragments":[{"text":"MLNMultiPoint","kind":"identifier"}],"title":"MLNMultiPoint"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"type":"topic","title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotations:":{"navigatorTitle":[{"text":"addAnnotations:","kind":"identifier"}],"type":"topic","kind":"symbol","title":"addAnnotations:","url":"\/documentation\/maplibre\/mlnmapview\/addannotations:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotations:","role":"symbol","abstract":[{"text":"Adds an array of annotations to the map view.","type":"text"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addAnnotations:"}]}}}