":"text"},{"code":"Info.plist","type":"codeVoice"},{"type":"text","text":" to satisfy the"},{"type":"text","text":" "},{"text":"requirements of the underlying Core Location framework when enabling this","type":"text"},{"text":" ","type":"text"},{"text":"property.","type":"text"}]},{"inlineContent":[{"type":"text","text":"If you implement a custom location manager, set the "},{"type":"codeVoice","code":"locationManager"},{"text":" before","type":"text"},{"type":"text","text":" "},{"type":"text","text":"calling "},{"code":"showsUserLocation","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"}],"kind":"content"}],"abstract":[{"text":"A Boolean value indicating whether the map may display the user location.","type":"text"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/showsuserlocation"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"symbol","schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/showsUserLocation","interfaceLanguage":"occ"},"metadata":{"symbolKind":"property","title":"showsUserLocation","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"showsUserLocation","kind":"identifier"}],"roleHeading":"Instance Property","role":"symbol","externalID":"c:objc(cs)MLNMapView(py)showsUserLocation","fragments":[{"kind":"identifier","text":"showsUserLocation"}]},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/showsUserLocation":{"abstract":[{"text":"A Boolean value indicating whether the map may display the user location.","type":"text"}],"title":"showsUserLocation","url":"\/documentation\/maplibre\/mlnmapview\/showsuserlocation","kind":"symbol","fragments":[{"kind":"identifier","text":"showsUserLocation"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/