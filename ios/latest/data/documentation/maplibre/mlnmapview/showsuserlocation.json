r"},{"text":" ","type":"text"},{"type":"codeVoice","code":"NSLocationAlwaysUsageDescription"},{"text":" in its ","type":"text"},{"code":"Info.plist","type":"codeVoice"},{"type":"text","text":" to satisfy the"},{"type":"text","text":" "},{"text":"requirements of the underlying Core Location framework when enabling this","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"property."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If you implement a custom location manager, set the "},{"type":"codeVoice","code":"locationManager"},{"type":"text","text":" before"},{"type":"text","text":" "},{"type":"text","text":"calling "},{"code":"showsUserLocation","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"}],"kind":"content"}],"kind":"symbol","schemaVersion":{"patch":0,"major":0,"minor":3},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"text":"A Boolean value indicating whether the map may display the user location.","type":"text"}],"metadata":{"fragments":[{"text":"showsUserLocation","kind":"identifier"}],"symbolKind":"property","role":"symbol","externalID":"c:objc(cs)MLNMapView(py)showsUserLocation","navigatorTitle":[{"text":"showsUserLocation","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Property","title":"showsUserLocation"},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/showsUserLocation"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","url":"\/documentation\/maplibre","role":"collection","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/sho