n"},{"text":" or","type":"text"},{"type":"text","text":" "},{"type":"codeVoice","code":"NSLocationAlwaysUsageDescription"},{"type":"text","text":" in its "},{"type":"codeVoice","code":"Info.plist"},{"type":"text","text":" to satisfy the"},{"text":" ","type":"text"},{"type":"text","text":"requirements of the underlying Core Location framework when enabling this"},{"text":" ","type":"text"},{"type":"text","text":"property."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you implement a custom location manager, set the "},{"code":"locationManager","type":"codeVoice"},{"type":"text","text":" before"},{"type":"text","text":" "},{"text":"calling ","type":"text"},{"code":"showsUserLocation","type":"codeVoice"},{"text":".","type":"text"}]}]}],"abstract":[{"type":"text","text":"A Boolean value indicating whether the map may display the user location."}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/showsuserlocation"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"metadata":{"fragments":[{"text":"showsUserLocation","kind":"identifier"}],"symbolKind":"property","title":"showsUserLocation","role":"symbol","externalID":"c:objc(cs)MLNMapView(py)showsUserLocation","roleHeading":"Instance Property","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"showsUserLocation","kind":"identifier"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","type":"topic","fragments":[{"text":"MLNMapView","kind":"identifier"}],"abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"text":"provided by App