lwaysUsageDescription"},{"text":" in its ","type":"text"},{"code":"Info.plist","type":"codeVoice"},{"text":" to satisfy the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"requirements of the underlying Core Location framework when enabling this"},{"type":"text","text":" "},{"type":"text","text":"property."}]},{"inlineContent":[{"type":"text","text":"If you implement a custom location manager, set the "},{"type":"codeVoice","code":"locationManager"},{"type":"text","text":" before"},{"text":" ","type":"text"},{"text":"calling ","type":"text"},{"code":"showsUserLocation","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"}],"kind":"content"}],"abstract":[{"type":"text","text":"A Boolean value indicating whether the map may display the user location."}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/showsuserlocation"]}],"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/showsUserLocation"},"kind":"symbol","metadata":{"symbolKind":"property","roleHeading":"Instance Property","role":"symbol","modules":[{"name":"MapLibre"}],"navigatorTitle":[{"kind":"identifier","text":"showsUserLocation"}],"fragments":[{"text":"showsUserLocation","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(py)showsUserLocation","title":"showsUserLocation"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","url":"\/documentation\/maplibre","title":"MapLibre","kind":"symbol","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/sho