type":"text","text":" or"},{"text":" ","type":"text"},{"type":"codeVoice","code":"NSLocationAlwaysUsageDescription"},{"text":" in its ","type":"text"},{"code":"Info.plist","type":"codeVoice"},{"type":"text","text":" to satisfy the"},{"type":"text","text":" "},{"text":"requirements of the underlying Core Location framework when enabling this","type":"text"},{"type":"text","text":" "},{"type":"text","text":"property."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"If you implement a custom location manager, set the "},{"code":"locationManager","type":"codeVoice"},{"text":" before","type":"text"},{"type":"text","text":" "},{"text":"calling ","type":"text"},{"type":"codeVoice","code":"showsUserLocation"},{"type":"text","text":"."}],"type":"paragraph"}],"kind":"content"}],"kind":"symbol","variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/showsuserlocation"],"traits":[{"interfaceLanguage":"occ"}]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/showsUserLocation"},"metadata":{"roleHeading":"Instance Property","title":"showsUserLocation","modules":[{"name":"MapLibre"}],"fragments":[{"text":"showsUserLocation","kind":"identifier"}],"role":"symbol","navigatorTitle":[{"text":"showsUserLocation","kind":"identifier"}],"symbolKind":"property","externalID":"c:objc(cs)MLNMapView(py)showsUserLocation"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/showsUserLocation":{"role":"symbol","title":"showsUserLocation","abstract":[{"type":"text","text":"A Boolean value indicating whether the map may display the user location."}],"fragments":[{"kind":"identifier","text":"showsUserLocation"}],"url":"\/documentation\/maplibre\/mlnmapview\/showsuserlocation","type":"topic","kind":"symbol","identifier":"doc:\/\/org.s