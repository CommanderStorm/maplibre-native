e":"text"},{"type":"text","text":"whether the user’s position is visible, use the "},{"type":"codeVoice","code":"userLocationVisible"},{"type":"text","text":" property."},{"type":"text","text":" "},{"type":"text","text":"The default value of this property is "},{"type":"codeVoice","code":"NO"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Your app must specify a value for "},{"type":"codeVoice","code":"NSLocationWhenInUseUsageDescription"},{"text":" or","type":"text"},{"type":"text","text":" "},{"code":"NSLocationAlwaysUsageDescription","type":"codeVoice"},{"type":"text","text":" in its "},{"type":"codeVoice","code":"Info.plist"},{"text":" to satisfy the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"requirements of the underlying Core Location framework when enabling this"},{"type":"text","text":" "},{"text":"property.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you implement a custom location manager, set the "},{"type":"codeVoice","code":"locationManager"},{"text":" before","type":"text"},{"type":"text","text":" "},{"type":"text","text":"calling "},{"type":"codeVoice","code":"showsUserLocation"},{"text":".","type":"text"}]}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"sections":[],"abstract":[{"type":"text","text":"A Boolean value indicating whether the map may display the user location."}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/showsuserlocation"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/showsUserLocation":{"fragments":[{"text":"showsUserLocation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/showsuserlocation","abstract":[{"text":"A Boolean value indicating whether the map may display the user location.","type":"text"}],"type":"topic","title":"showsUserLocation","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/