{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"text":"Removes an annotation from the map view, deselecting it if it is selected.","type":"text"}],"kind":"symbol","metadata":{"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"}],"title":"removeAnnotation:","modules":[{"name":"MapLibre"}],"role":"symbol","symbolKind":"method","navigatorTitle":[{"text":"removeAnnotation:","kind":"identifier"}],"roleHeading":"Instance Method","externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:"},"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:"},"schemaVersion":{"minor":3,"patch":0,"major":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","text":"void","preciseIdentifier":"c:v"},{"text":") ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation","text":"id<MLNAnnotation>","kind":"typeIdentifier"},{"kind":"text","text":") "},{"text":"annotation","kind":"internalParam"},{"text":";","kind":"text"}]}]},{"parameters":[{"name":"annotation","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation object to remove. This object must conform"},{"text":" ","type":"text"},{"type":"text","text":"to the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"inlineContent":[{"text":"Removing an annotation object dissociates it from the map view entirely,","type":"text"},{"text":" ","type":"text