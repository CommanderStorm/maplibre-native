entation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotation:":{"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/removeannotation:","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"}],"navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"title":"removeAnnotation:","abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","kind":"symbol","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview","type":"topic","fragments":[{"kind":"identifier","text":"MLNMapView"}]}}}