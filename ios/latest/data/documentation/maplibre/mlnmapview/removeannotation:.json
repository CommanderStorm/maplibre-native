nce Method","navigatorTitle":[{"text":"removeAnnotation:","kind":"identifier"}],"title":"removeAnnotation:","modules":[{"name":"MapLibre"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:"},"schemaVersion":{"minor":3,"patch":0,"major":0},"abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}],"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"title":"MLNMapView","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}],"kind":"symbol","role":"symbol","title":"removeAnnotation:","navigatorTitle":[{"text":"removeAnnotation:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/removeannotation:","type":"topic","fragments":[{"kind":"text","text":"- "},{"text":"removeAnnotation:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","url":"\/documentation\/maplibre","role":"collection","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","title":"MLNAnnotation"}}}