{"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/removeannotation:"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"patch":0,"minor":3,"major":0},"sections":[],"metadata":{"fragments":[{"kind":"text","text":"- "},{"text":"removeAnnotation:","kind":"identifier"}],"symbolKind":"method","navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:","roleHeading":"Instance Method","role":"symbol","title":"removeAnnotation:","modules":[{"name":"MapLibre"}]},"kind":"symbol","identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:"},"abstract":[{"text":"Removes an annotation from the map view, deselecting it if it is selected.","type":"text"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"kind":"identifier","text":"removeAnnotation:"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"id<MLNAnnotation>","preciseIdentifier":"c:Qoobjc(pl)MLNAnnotation"},{"text":") ","kind":"text"},{"text":"annotation","kind":"internalParam"},{"text":";","kind":"text"}],"languages":["occ"],"platforms":["iOS"]}]},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The annotation object to remove. This object must conform"},{"text":" ","type":"text"},{"type":"text","text":"to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"text":" protocol","type":"text"}]}],"name":"annotation"}],"kind":"parameters"},{"content":[{"text":"Discussion","level":2,"anchor":"discussion","type":"heading"},{"inlineContent":[{"type":"text","