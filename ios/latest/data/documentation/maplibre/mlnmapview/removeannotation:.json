igatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","symbolKind":"method","fragments":[{"text":"- ","kind":"text"},{"text":"removeAnnotation:","kind":"identifier"}],"title":"removeAnnotation:","role":"symbol","externalID":"c:objc(cs)MLNMapView(im)removeAnnotation:"},"sections":[],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"kind":"identifier","text":"MLNMapView"}],"url":"\/documentation\/maplibre\/mlnmapview","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"kind":"symbol","role":"symbol","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotation:":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/removeannotation:","navigatorTitle":[{"kind":"identifier","text":"removeAnnotation:"}],"abstract":[{"type":"text","text":"Removes an annotation from the map view, deselecting it if it is selected."}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotation:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotation:","title":"removeAnnotation:","type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent an