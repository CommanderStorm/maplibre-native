entation\/maplibre\/mlnmapview","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"type":"topic","role":"symbol","title":"addAnnotation:","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"addAnnotation:"}],"url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","kind":"symbol","abstract":[{"type":"text","text":"Adds an annotation to the map view."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","type":"topic","role":"symbol","title":"MLNPointCollection","fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection"},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances. The"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"type":"text","text":" "},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"orchard."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","kind":"symbol","role":"collection","url":"\/documentation\/maplibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"topic","role":"symbol","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}]}}}