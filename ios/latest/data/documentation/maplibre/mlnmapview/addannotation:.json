dentifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"navigatorTitle":[{"text":"addAnnotation:","kind":"identifier"}],"title":"addAnnotation:","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","type":"topic","fragments":[{"kind":"text","text":"- "},{"text":"addAnnotation:","kind":"identifier"}],"abstract":[{"type":"text","text":"Adds an annotation to the map view."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","fragments":[{"kind":"identifier","text":"MLNMultiPolyline"}],"title":"MLNMultiPolyline","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","type":"reference","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"text":" ","type":"text"},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"type":"text","text":" object."}],"role":"symbol","url":"\/documentation\/maplibre\/mlnmultipolyline","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","type":"topic","title":"MLNAnnotation","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","url":"\/documentation\/maplibre","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","kind":"symbol"}}}