yProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","role":"symbol","kind":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"title":"MLNMultiPolyline","type":"topic","url":"\/documentation\/maplibre\/mlnmultipolyline","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"text":" object.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"title":"addAnnotation:","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","fragments":[{"text":"- ","kind":"text"},{"text":"addAnnotation:","kind":"identifier"}],"navigatorTitle":[{"text":"addAnnotation:","kind":"identifier"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","type":"topic","abstract":[{"text":"Adds an annotation to the map view.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"fragments":[{"text":"MLNMapView","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","role":"symbol","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"reference","isActive":true},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"text":" instances, and the line segments that","type":"text"},{"text":" ","type":"text"},{"text":"connect them. For example, you could use an polyline to represent a road or the","type":"text"},{"text":" ","type":"text"},{"text":"path along which something moves.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNPolyline"}],"title":"MLNPolyline","kind":"symbol","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnpolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"text":" object represents a shape consisting of one or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"disconnected vertices, specified as "},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances. The"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"text":" ","type":"text"},{"text":"orchard.","type":"text"}],"role":"symbol","title":"MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","fragments":[{"text":"MLNPointCollection","kind":"identifier"}]}}}