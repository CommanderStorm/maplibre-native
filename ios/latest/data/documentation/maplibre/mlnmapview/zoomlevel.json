Voice"},{"text":" method instead.","type":"text"}],"type":"paragraph"}]}],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/zoomlevel"],"traits":[{"interfaceLanguage":"occ"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"kind":"symbol","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"provided by Apple’s MapKit."}],"title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","fragments":[{"text":"MLNMapView","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapview","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","title":"MapLibre","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"kind":"symbol","abstract":[{"type":"text","text":"The zoom level of the receiver."}],"title":"zoomLevel","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel","fragments":[{"text":"zoomLevel","kind":"identifier"}],"type":"topic","url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","role":"symbol"}}}