re\/mlnmapview\/zoomlevel"],"traits":[{"interfaceLanguage":"occ"}]}],"abstract":[{"type":"text","text":"The zoom level of the receiver."}],"schemaVersion":{"patch":0,"major":0,"minor":3},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","title":"MapLibre","type":"topic","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/zoomLevel":{"type":"topic","kind":"symbol","role":"symbol","title":"zoomLevel","url":"\/documentation\/maplibre\/mlnmapview\/zoomlevel","fragments":[{"text":"zoomLevel","kind":"identifier"}],"abstract":[{"type":"text","text":"The zoom level of the receiver."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/zoomLevel"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview","type":"topic","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}]}}}