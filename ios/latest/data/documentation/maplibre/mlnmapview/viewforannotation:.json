cumentation/MapLibre/MLNMapView/viewForAnnotation:":{"role":"symbol","type":"topic","title":"viewForAnnotation:","url":"\/documentation\/maplibre\/mlnmapview\/viewforannotation:","abstract":[{"type":"text","text":"Returns an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" if the given annotation is currently associated","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with a view, otherwise nil."}],"navigatorTitle":[{"kind":"identifier","text":"viewForAnnotation:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/viewForAnnotation:","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"viewForAnnotation:"}]}}}