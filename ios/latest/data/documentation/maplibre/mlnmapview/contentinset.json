n deprecated"},{"text":" ","type":"text"},{"type":"text","text":"use the map view’s property "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/automaticallyAdjustsContentInset"},{"type":"text","text":"instead."}]},{"type":"paragraph","inlineContent":[{"text":"Changing the value of this property updates the map view immediately. If you","type":"text"},{"type":"text","text":" "},{"text":"want to animate the change, use the ","type":"text"},{"code":"-setContentInset:animated:completionHandler:","type":"codeVoice"},{"text":" ","type":"text"},{"type":"text","text":"method instead."}]}],"kind":"content"}],"metadata":{"externalID":"c:objc(cs)MLNMapView(py)contentInset","roleHeading":"Instance Property","symbolKind":"property","fragments":[{"text":"contentInset","kind":"identifier"}],"modules":[{"name":"MapLibre"}],"navigatorTitle":[{"text":"contentInset","kind":"identifier"}],"role":"symbol","title":"contentInset"},"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnmapview\/contentinset"],"traits":[{"interfaceLanguage":"occ"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"abstract":[{"text":"The distance from the edges of the map view’s frame to the edges of the map","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"view’s logical viewport."}],"schemaVersion":{"patch":0,"minor":3,"major":0},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/contentInset","interfaceLanguage":"occ"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","kind":"symbol","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type"