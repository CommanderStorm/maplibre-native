{"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["occ"],"tokens":[{"kind":"text","text":"- ("},{"kind":"typeIdentifier","preciseIdentifier":"c:v","text":"void"},{"kind":"text","text":") "},{"text":"removeAnnotations:","kind":"identifier"},{"text":"(","kind":"text"},{"text":"NSArray<id<MLNAnnotation>>","kind":"typeIdentifier","preciseIdentifier":"c:Q$objc(cs)NSArray"},{"text":" *) ","kind":"text"},{"text":"annotations","kind":"internalParam"},{"kind":"text","text":";"}]}],"kind":"declarations"},{"parameters":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The array of annotation objects to remove. Objects in the"},{"type":"text","text":" "},{"type":"text","text":"array must conform to the "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol."}]}],"name":"annotations"}],"kind":"parameters"},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"text":"Removing annotation objects dissociates them from the map view entirely,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"preventing them from being displayed on the map. Thus you would typically"},{"text":" ","type":"text"},{"type":"text","text":"call this method only when you want to hide or delete the given annotations."}]}]}],"abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"text":" ","type":"text"},{"type":"text","text":"annotations in the array."}],"kind":"symbol","sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","interfaceLanguage":"oc