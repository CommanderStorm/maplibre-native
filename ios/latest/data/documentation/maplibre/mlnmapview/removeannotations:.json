erarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"sections":[],"kind":"symbol","abstract":[{"text":"Removes an array of annotations from the map view, deselecting any selected","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"annotations in the array."}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/removeannotations:"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotations:"}],"url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","abstract":[{"text":"Removes an array of annotations from the map view, deselecting any selected","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"title":"removeAnnotations:","kind":"symbol","role":"symbol","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"type":"topic","role":"symbol","title":"MLNMapView","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"text":"provided by Apple’s MapKit.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNMapView"}],"url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","url":"\/documentation\/maplibre","title":"MapLibre","role":"collection","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"url":"\/documentation\/maplibre\/mlnannotation","role":"symbol","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","title":"MLNAnnotation"}}}