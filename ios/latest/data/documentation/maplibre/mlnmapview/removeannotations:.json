nd":"content","content":[{"anchor":"discussion","text":"Discussion","type":"heading","level":2},{"type":"paragraph","inlineContent":[{"type":"text","text":"Removing annotation objects dissociates them from the map view entirely,"},{"text":" ","type":"text"},{"type":"text","text":"preventing them from being displayed on the map. Thus you would typically"},{"text":" ","type":"text"},{"text":"call this method only when you want to hide or delete the given annotations.","type":"text"}]}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removeAnnotations:"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","role":"symbol","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"title":"removeAnnotations:","type":"topic","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"type":"text","text":" "},{"text":"annotations in the array.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","type":"topic","role":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","url":"\/documentation\/maplibre","kind":"symbol","title":"MapLibre","role":"collection","type":"topic","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with