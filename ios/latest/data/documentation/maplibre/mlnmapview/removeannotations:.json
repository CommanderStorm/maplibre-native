ibre\/mlnmapview\/removeannotations:"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"minor":3,"major":0,"patch":0},"metadata":{"roleHeading":"Instance Method","externalID":"c:objc(cs)MLNMapView(im)removeAnnotations:","role":"symbol","modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotations:"}],"navigatorTitle":[{"kind":"identifier","text":"removeAnnotations:"}],"title":"removeAnnotations:","symbolKind":"method"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"type":"topic","abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"type":"text","text":" "},{"text":"annotations in the array.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","kind":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotations:"}],"navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"title":"removeAnnotations:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"text":" ","type":"text"},{"text":"provided by Apple’s MapKit.","type":"text"}],"url":"\/documentation\/maplibre\/mlnmapview","title":"MLNMapView","role":"symbol","type":"topic","fragments":[{"text":"MLNMapView","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation