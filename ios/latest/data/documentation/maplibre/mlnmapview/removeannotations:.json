ifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","interfaceLanguage":"occ"},"metadata":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"removeAnnotations:"}],"title":"removeAnnotations:","roleHeading":"Instance Method","navigatorTitle":[{"kind":"identifier","text":"removeAnnotations:"}],"role":"symbol","symbolKind":"method","modules":[{"name":"MapLibre"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotations:"},"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","title":"MapLibre","kind":"symbol","url":"\/documentation\/maplibre","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"kind":"symbol","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"removeAnnotations:","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"title":"removeAnnotations:","abstract":[{"text":"Removes an array of annotations from the map view, deselecting any selected","type":"text"},{"type":"text","text":" "},{"type":"text","text":"annotations in the array."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","title":"MLNMapView","kind":"symbol","fragments":[{"text":"MLNMapView","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnmapview","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"role":"symbol","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"type":"topic","title":"MLNAnnotation","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation"}}}