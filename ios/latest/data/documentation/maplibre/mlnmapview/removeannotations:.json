{"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnmapview\/removeannotations:"]}],"sections":[],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"]]},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"text","text":"- ("},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"text":") ","kind":"text"},{"text":"removeAnnotations:","kind":"identifier"},{"kind":"text","text":"("},{"preciseIdentifier":"c:Q$objc(cs)NSArray","text":"NSArray<id<MLNAnnotation>>","kind":"typeIdentifier"},{"kind":"text","text":" *) "},{"kind":"internalParam","text":"annotations"},{"text":";","kind":"text"}],"languages":["occ"]}],"kind":"declarations"},{"kind":"parameters","parameters":[{"name":"annotations","content":[{"inlineContent":[{"type":"text","text":"The array of annotation objects to remove. Objects in the"},{"type":"text","text":" "},{"type":"text","text":"array must conform to the "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol.","type":"text"}],"type":"paragraph"}]}]},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"Removing annotation objects dissociates them from the map view entirely,","type":"text"},{"text":" ","type":"text"},{"text":"preventing them from being displayed on the map. Thus you would typically","type":"text"},{"type":"text","text":" "},{"type":"text","text":"call this method only when you want to hide or delete the given annotations."}]}],"kind":"content"}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","interfaceLanguage":"occ"},"kind":"symbol","metadata":{"modules":[{"name":"MapLibre"}],"fragments":[{"text":"- ","kind":"text"},{"text":"removeAnnotations:","kind":"identifier"}],"title":"removeAnnotations:","role":"symbol","symbolKind":"method","roleHeading":"Instance Method","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}],"externalID":"c:objc(cs)MLNMapView(im)removeAnnotations:"},"schemaVersion":{"patch":0,"major":0,"minor":3},"abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"text":" ","type":"text"},{"text":"annotations in the array.","type":"text"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","role":"collection","type":"topic","title":"MapLibre","url":"\/documentation\/maplibre","kind":"symbol","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"type":"text","text":"The "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"url":"\/documentation\/maplibre\/mlnmapview","kind":"symbol","role":"symbol","type":"topic","title":"MLNMapView","abstract":[{"text":"An interactive, customizable map view with an interface similar to the one","type":"text"},{"type":"text","text":" "},{"type":"text","text":"provided by Apple’s MapKit."}],"fragments":[{"text":"MLNMapView","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/removeAnnotations:":{"fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"removeAnnotations:"}],"url":"\/documentation\/maplibre\/mlnmapview\/removeannotations:","title":"removeAnnotations:","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/removeAnnotations:","abstract":[{"type":"text","text":"Removes an array of annotations from the map view, deselecting any selected"},{"text":" ","type":"text"},{"type":"text","text":"annotations in the array."}],"type":"topic","kind":"symbol","navigatorTitle":[{"text":"removeAnnotations:","kind":"identifier"}]}}}