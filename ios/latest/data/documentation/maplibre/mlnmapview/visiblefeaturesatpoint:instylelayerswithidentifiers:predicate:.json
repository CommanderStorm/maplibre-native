":[{"kind":"identifier","text":"MLNAnnotation"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNRasterStyleLayer":{"fragments":[{"text":"MLNRasterStyleLayer","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnrasterstylelayer","abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer"},{"type":"text","text":" is a style layer that renders georeferenced raster"},{"type":"text","text":" "},{"text":"imagery on the map, especially raster tiles.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","type":"topic","role":"symbol","title":"MLNRasterStyleLayer"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/visibleAnnotationsInRect:":{"url":"\/documentation\/maplibre\/mlnmapview\/visibleannotationsinrect:","type":"topic","role":"symbol","abstract":[{"type":"text","text":"Returns the list of annotations associated with the receiver that intersect with"},{"type":"text","text":" "},{"type":"text","text":"the given rectangle."}],"navigatorTitle":[{"kind":"identifier","text":"visibleAnnotationsInRect:"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleAnnotationsInRect:","fragments":[{"text":"- ","kind":"text"},{"text":"visibleAnnotationsInRect:","kind":"identifier"}],"title":"visibleAnnotationsInRect:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource/featuresMatchingPredicate:":{"role":"symbol","abstract":[{"type":"text","text":"Returns an array of map features for this source, filtered by the given"},{"text":" ","type":"text"},{"type":"text","text":"predicate."}],"url":"\/documentation\/maplibre\/mlnshapesource\/featuresmatchingpredicate:","title":"featuresMatchingPredicate:","kind":"symbol","fragments":[{"text":"- ","kind":"text"},{"text":"featuresMatchingPredicate:","kind":"identifier"}],"navigatorTitle":[{"text":"featuresMatchingPredicate:","kind":"identifier"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/featuresMatchingPredicate:"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView":{"title":"MLNMapView","type":"topic","abstract":[{"type":"text","text":"An interactive, customizable map view with an interface similar to the one"},{"type":"text","text":" "},{"type":"text","text":"provided by Appleâ€™s MapKit."}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNMapView"}],"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView","url":"\/documentation\/maplibre\/mlnmapview"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"type":"topic","title":"MLNShape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","kind":"symbol","fragments":[{"text":"MLNShape","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnshape","role":"symbol","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"text":"constitute the content of a map â€” not only the overlays atop the map, but also","type":"text"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}]}}}