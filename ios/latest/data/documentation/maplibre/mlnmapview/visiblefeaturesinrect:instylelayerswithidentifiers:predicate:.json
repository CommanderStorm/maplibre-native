ctive":true},{"type":"text","text":" or "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":",","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"or they may be defined by local or external"},{"type":"text","text":" "},{"type":"text","text":"GeoJSON"},{"type":"text","text":" code. A shape source is added to an"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"type":"text","text":" object along with an "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","type":"reference"},{"text":" object. The vector style","type":"text"},{"text":" ","type":"text"},{"text":"layer defines the appearance of any content supplied by the shape source. You","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"can update a shape source by setting its "},{"code":"shape","type":"codeVoice"},{"text":" or ","type":"text"},{"code":"URL","type":"codeVoice"},{"text":" property.","type":"text"}],"kind":"symbol","url":"\/documentation\/maplibre\/mlnshapesource","type":"topic","title":"MLNShapeSource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeSource"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"title":"MLNFeature","fragments":[{"kind":"identifier","text":"MLNFeature"}],"url":"\/documentation\/maplibre\/mlnfeature","type":"topic","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","kind":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true,"type":"reference"},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"type":"text","text":" "},{"type":"text","text":"contained in an "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource","isActive":true},{"type":"text","text":" or "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true,"type":"reference"},{"type":"text","text":" object. Each concrete"},{"text":" ","type":"text"},{"type":"text","text":"subclass of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"text":" in turn has a subclass that conforms to this protocol. A","type":"text"},{"text":" ","type":"text"},{"text":"feature object associates a shape with an optional identifier and attributes.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","abstract":[{"type":"text","text":"Returns an array of rendered map features that intersect with the given"},{"text":" ","type":"text"},{"text":"rectangle, restricted to the given style layers and filtered by the given","type":"text"},{"type":"text","text":" "},{"text":"predicate.","type":"text"}],"kind":"symbol","title":"visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","navigatorTitle":[{"text":"visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:","kind":"identifier"}],"role":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"visibleFeaturesInRect:inStyleLayersWithIdentifiers:predicate:"}],"url":"\/documentation\/maplibre\/mlnmapview\/visiblefeaturesinrect:instylelayerswithidentifiers:predicate:","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeSource/featuresMatchingPredicate:":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource\/featuresMatchingPredicate:"