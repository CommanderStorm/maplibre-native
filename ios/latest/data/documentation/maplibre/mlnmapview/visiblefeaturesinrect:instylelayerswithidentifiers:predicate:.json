pe":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true},{"text":" object along with one or more","type":"text"},{"text":" ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer"},{"text":" objects. Use a raster style layer to control the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"appearance of content supplied by the raster tile source."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","url":"\/documentation\/maplibre\/mlnrastertilesource","title":"MLNRasterTileSource","kind":"symbol","type":"topic","fragments":[{"text":"MLNRasterTileSource","kind":"identifier"}]}}}