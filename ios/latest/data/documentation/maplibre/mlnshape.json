//org.swift.MyProject/documentation/MapLibre/MLNShape":{"url":"\/documentation\/maplibre\/mlnshape","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","fragments":[{"text":"MLNShape","kind":"identifier"}],"kind":"symbol","type":"topic","abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"type":"text","text":" "},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"type":"text","text":" "},{"text":"the content that forms the base map.","type":"text"}],"role":"symbol","title":"MLNShape"},"doc://org.swift.MyProject/documentation/MapLibre":{"url":"\/documentation\/maplibre","abstract":[{"text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","type":"topic","title":"MapLibre","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","kind":"symbol","role":"symbol","title":"MLNStyle","fragments":[{"kind":"identifier","text":"MLNStyle"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"title":"MLNPolyline","url":"\/documentation\/maplibre\/mlnpolyline","type":"topic","kind":"symbol","abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of two or more vertices,"},{"text":" ","type":"text"},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the line segments that"},{"type":"text","text":" "},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"text":"path along which something moves.","type":"text"}],"role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","role":"symbol","type":"topic","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"text":" ","type":"text"},{"text":"objects to handle the display.","type":"text"}],"fragments":[{"kind":"identifier","text":"MLNAnnotation"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","kind":"symbol","title":"MLNMultiPolyline","role":"symbol","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","isActive":true},{"text":" object represents a shape consisting of one or more","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"polylines. For example, you could use a multipolyline shape to represent both"},{"type":"text","text":" "},{"text":"sides of a divided highway (dual carriageway), excluding the median (central","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","isActive":true},{"text":" object.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/subtitle":{"kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle","title":"subtitle","abstract":[{"text":"The subtitle of the shape annotation. The default value of this property is","type":"text"},{"text":" ","type":"text"},{"code":"nil","type":"codeVoice"},{"type":"text","text":"."}],"fragments":[{"kind":"identifier","text":"subtitle"}],"url":"\/documentation\/maplibre\/mlnshape\/subtitle","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","url":"\/documentation\/maplibre\/mlnpolylinefeature","kind":"symbol","title":"MLNPolylineFeature","role":"symbol","fragments":[{"kind":"identifier","text":"MLNPolylineFeature"}],"abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\