type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"type":"text","text":" object, while the surrounding"},{"text":" ","type":"text"},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"type":"text","text":" "},{"type":"text","text":"group of disconnected but related buildings."}],"title":"MLNMultiPolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","role":"symbol","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNEmptyFeature":{"url":"\/documentation\/maplibre\/mlnemptyfeature","kind":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","isActive":true},{"type":"text","text":" object associates an empty shape with an optional"},{"type":"text","text":" "},{"type":"text","text":"identifier and attributes."}],"title":"MLNEmptyFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNEmptyFeature","role":"symbol","fragments":[{"text":"MLNEmptyFeature","kind":"identifier"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"url":"\/documentation\/maplibre\/mlnshapecollection","kind":"symbol","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"reference"},{"type":"text","text":" object represents a shape consisting of zero or more"},{"text":" ","type":"text"},{"type":"text","text":"distinct but related shapes that are instances of "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"type":"text","text":". The constituent"},{"type":"tex