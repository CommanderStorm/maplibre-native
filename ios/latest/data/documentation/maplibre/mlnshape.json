"type":"topic","url":"\/documentation\/maplibre\/mlnshapesource","kind":"symbol","fragments":[{"text":"MLNShapeSource","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","title":"MLNStyle","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnstyle","fragments":[{"text":"MLNStyle","kind":"identifier"}],"abstract":[{"text":"The proxy object for the current map style.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/objc(pl)NSSecureCoding":{"title":"NSSecureCoding","identifier":"doc:\/\/org.swift.MyProject\/objc(pl)NSSecureCoding","type":"unresolvable"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygonFeature":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygonfeature","fragments":[{"kind":"identifier","text":"MLNMultiPolygonFeature"}],"abstract":[{"text":"An ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature"},{"type":"text","text":" object associates a multipolygon shape with an"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"type":"topic","title":"MLNMultiPolygonFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygonFeature","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolygon":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnmultipolygon","fragments":[{"text":"MLNMultiPolygon","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","isActive":true},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"text":"polygons that do not overlap. For example, you could use a multipolygon shape","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to represent the body of land that consists of an island surrounded by an"},{"text":" ","type":"text"},{"type":"text","text":"atoll: the inner island would be one "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon"},{"text":" object, while the surrounding","type":"text"},{"type":"text","text":" "},{"text":"atoll would be another. You could also use a multipolygon shape to represent a","type":"text"},{"text":" ","type":"text"},{"text":"group of disconnected but related buildings.","type":"text"}],"type":"topic","title":"MLNMultiPolygon","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","title":"MLNPointAnnotation","role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnpointannotation","fragments":[{"text":"MLNPointAnnotation","kind":"identifier"}],"abstract":[{"text":"An ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","isActive":true,"type":"reference"},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, you could use a point shape to represent a city at low zoom levels, an"},{"type":"text","text":" "},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNFeature":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","url":"\/documentation\/maplibre\/mlnfeature","role":"symbol","fragments":[{"text":"MLNFeature","kind":"identifier"}],"type":"topic","title":"MLNFeature","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNFeature","isActive":true},{"type":"text","text":"  protocol is used to provide details about geographic features"},{"type":"text","text":" "},{"type":"text","text":"contained in an "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeSource"},{"text":" or ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","type":"reference"},{"type":"text","text":" object. Each concrete"},{"type":"text","text":" "},{"text":"subclass of ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true},{"text":" in turn has a subclass that conforms to this protocol. A","type":"text"},{"text":" ","type":"text"},{"text":"feature object associates a shape with an optional identifier and attributes.","type":"text"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollectionFeature":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnpointcollectionfeature","fragments":[{"kind":"identifier","text":"MLNPointCollectionFeature"}],"abstract":[{"text":"An ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","isActive":true},{"type":"text","text":" object associates a point collection with an"},{"type":"text","text":" "},{"text":"optional identifier and attributes.","type":"text"}],"type":"topic","title":"MLNPointCollectionFeature","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature","role":"symbol"}}}