lygon","role":"symbol","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolygon","type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"type":"text","text":"polygons that do not overlap. For example, you could use a multipolygon shape"},{"text":" ","type":"text"},{"text":"to represent the body of land that consists of an island surrounded by an","type":"text"},{"text":" ","type":"text"},{"text":"atoll: the inner island would be one ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolygon","isActive":true},{"type":"text","text":" object, while the surrounding"},{"text":" ","type":"text"},{"type":"text","text":"atoll would be another. You could also use a multipolygon shape to represent a"},{"text":" ","type":"text"},{"type":"text","text":"group of disconnected but related buildings."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","url":"\/documentation\/maplibre\/mlnshape","title":"MLNShape","type":"topic","abstract":[{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","type":"reference"},{"text":" is an abstract class that represents a shape or annotation. Shapes","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"constitute the content of a map — not only the overlays atop the map, but also"},{"text":" ","type":"text"},{"text":"the content that forms the base map.","type":"text"}],"kind":"symbol","fragments":[{"kind":"identifier","text":"MLNShape"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"abstract":[{"text":"The ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"text":" ","type":"text"},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","kind":"symbol","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolylineFeature":{"title":"MLNPolylineFeature","url":"\/documentation\/maplibre\/mlnpolylinefeature","abstract":[{"type":"text","text":"An "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","isActive":true},{"text":" object associates a polyline shape with an optional","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"identifier and attributes."}],"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolylineFeature","kind":"symbol","type":"topic","fragments":[{"text":"MLNPolylineFeature","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNShape/subtitle":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape\/subtitle","url":"\/documentation\/maplibre\/mlnshape\/subtitle","role":"symbol","kind":"symbol","title":"subtitle","type":"topic","fragments":[{"kind":"identifier","text":"subtitle"}],"abstract":[{"type":"text","text":"The subtitle of the shape annotation. The default value of this property is"},{"text":" ","type":"text"},{"type":"codeVoice","code":"nil"},{"text":".","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNStyle":{"role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","url":"\/documentation\/maplibre\/mlnstyle","title":"MLNStyle","abstract":[{"type":"text","text":"The proxy object for the current map style."}],"type":"topic","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNStyle"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointAnnotation":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"type":"text","text":" object represents a one-dimensional shape located at a"},{"type":"text","text":" "},{"text":"single geographical coordinate. Depending on how it is used, an","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation","type":"reference","isActive":true},{"text":" object is known as a point annotation or point shape. For","type":"text"},{"text":" ","type":"text"},{"text":"example, you could use a point shape to represent a city at low zoom levels, an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"address at high zoom levels, or the location of a long press gesture."}],"type":"topic","title":"MLNPointAnnotation","url":"\/documentation\/maplibre\/mlnpointannotation","role":"symbol","kind":"symbol","fragments":[{"kind":"identifier","text":"MLNPointAnnotation"}]}}}