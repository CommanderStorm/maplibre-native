:{"fragments":[{"kind":"identifier","text":"MLNRasterTileSource"}],"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnrastertilesource","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource","title":"MLNRasterTileSource","abstract":[{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterTileSource"},{"text":" is a map content source that supplies raster image tiles","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the"},{"text":" ","type":"text"},{"type":"text","text":"TileJSON specification"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A raster tile source is added to an "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle"},{"text":" object along with one or more","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNRasterStyleLayer","isActive":true,"type":"reference"},{"type":"text","text":" objects. Use a raster style layer to control the"},{"text":" ","type":"text"},{"text":"appearance of content supplied by the raster tile source.","type":"text"}],"type":"topic"}}}