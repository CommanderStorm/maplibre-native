ifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"title":"MLNAnnotationImage","fragments":[{"kind":"identifier","text":"MLNAnnotationImage"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"code":"UIImage","type":"codeVoice"},{"text":" ","type":"text"},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"text":" ","type":"text"},{"type":"text","text":"by the map view."}],"type":"topic","kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","url":"\/documentation\/maplibre\/mlnannotationimage","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"title":"rotatesToMatchCamera","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/rotatesToMatchCamera","abstract":[{"text":"A Boolean value that determines whether the annotation view rotates together","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with the map."}],"url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","fragments":[{"kind":"identifier","text":"rotatesToMatchCamera"}],"type":"topic","role":"symbol","kind":"symbol"}}}