yProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"modules":[{"name":"MapLibre"}],"symbolKind":"property","role":"symbol","externalID":"c:objc(cs)MLNAnnotationView(py)rotatesToMatchCamera","remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L193","fileName":"MLNAnnotationView.h"},"fragments":[{"text":"rotatesToMatchCamera","kind":"identifier"}],"title":"rotatesToMatchCamera","navigatorTitle":[{"text":"rotatesToMatchCamera","kind":"identifier"}],"roleHeading":"Instance Property"},"kind":"symbol","sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"role":"collection","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","kind":"symbol","url":"\/documentation\/maplibre","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"title":"MLNAnnotation","role":"symbol","kind":"symbol","type":"topic","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","role":"symbol","type":"topic","abstract":[{"type":"text","text":"A Boolean value that determines whether the annotation view rotates together"},{"type":"text","text":" "},{"text":"with the map.","type":"text"}],"kind":"symbol","title":"rotatesToMatchCamera","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/rotatesToMatchCamera","fragments":[{"kind":"identifier","text":"rotatesToMatchCamera"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"type":"text","text":" class is responsible for marking a point annotation"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"type":"text","text":" protocol. When an annotation’s"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"text":"offscreen.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview","title":"MLNAnnotationView","role":"symbol","kind":"symbol","type":"topic","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","role":"symbol","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"url":"\/documentation\/maplibre\/mlnannotationimage","kind":"symbol","title":"MLNAnnotationImage","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"text":"annotations visually on a map view. Annotation image objects wrap ","type":"text"},{"code":"UIImage","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"text":" ","type":"text"},{"type":"text","text":"by the map view."}]}}}