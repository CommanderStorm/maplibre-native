{"schemaVersion":{"major":0,"minor":3,"patch":0},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"@property"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"kind":"text","text":", "},{"kind":"keyword","text":"assign"},{"kind":"text","text":") "},{"text":"BOOL","kind":"typeIdentifier","preciseIdentifier":"c:@T@BOOL"},{"kind":"text","text":" "},{"text":"rotatesToMatchCamera","kind":"identifier"},{"text":";","kind":"text"}],"languages":["occ"]}],"kind":"declarations"},{"content":[{"level":2,"anchor":"discussion","type":"heading","text":"Discussion"},{"inlineContent":[{"text":"When the value of this property is ","type":"text"},{"code":"YES","type":"codeVoice"},{"text":" and the map is rotated, the annotation","type":"text"},{"type":"text","text":" "},{"text":"view rotates. This is also the behavior of ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference"},{"text":" objects. When the","type":"text"},{"text":" ","type":"text"},{"text":"value of this property is ","type":"text"},{"code":"NO","type":"codeVoice"},{"text":" the annotation has its rotation angle fixed.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"text":"The default value of this property is ","type":"text"},{"type":"codeVoice","code":"NO"},{"text":". Set this property to ","type":"text"},{"code":"YES","type":"codeVoice"},{"type":"text","text":" if the"},{"type":"text","text":" "},{"text":"view’s rotation is important.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera"]}],"sections":[],"abstract":[{"type":"text","text":"A Boolean value that determines whether the annotation view rotates together"},{"text":" ","type":"text"},{"type":"text","text":"with the map."}],"metadata":{"roleHeading":"Instance Property",