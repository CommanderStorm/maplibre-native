otation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/rotatesToMatchCamera","kind":"symbol","fragments":[{"kind":"identifier","text":"rotatesToMatchCamera"}],"title":"rotatesToMatchCamera","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","abstract":[{"text":"A Boolean value that determines whether the annotation view rotates together","type":"text"},{"text":" ","type":"text"},{"text":"with the map.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"abstract":[{"text":"The ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","isActive":true},{"type":"text","text":" class is responsible for presenting point-based"},{"text":" ","type":"text"},{"text":"annotations visually on a map view. Annotation image objects wrap ","type":"text"},{"code":"UIImage","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"text":" ","type":"text"},{"type":"text","text":"by the map view."}],"role":"symbol","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}],"title":"MLNAnnotationImage","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage"}}}