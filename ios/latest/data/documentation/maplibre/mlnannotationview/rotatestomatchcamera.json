s":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera"],"traits":[{"interfaceLanguage":"occ"}]}],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/rotatesToMatchCamera","interfaceLanguage":"occ"},"metadata":{"modules":[{"name":"MapLibre"}],"symbolKind":"property","roleHeading":"Instance Property","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L193"},"title":"rotatesToMatchCamera","externalID":"c:objc(cs)MLNAnnotationView(py)rotatesToMatchCamera","role":"symbol","fragments":[{"kind":"identifier","text":"rotatesToMatchCamera"}],"navigatorTitle":[{"kind":"identifier","text":"rotatesToMatchCamera"}]},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage","title":"MLNAnnotationImage","fragments":[{"kind":"identifier","text":"MLNAnnotationImage"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference","isActive":true},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"text":"annotations visually on a map view. Annotation image objects wrap ","type":"text"},{"type":"codeVoice","code":"UIImage"},{"text":" ","type":"text"},{"type":"text","text":"objects and may be recycled later and put into a reuse queue that is maintained"},{"text":" ","type":"text"},{"text":"by the map view.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","fragments":[{"text":"rotatesToMatchCamera","kind":"identifier"}