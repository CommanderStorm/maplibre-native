ifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/rotatesToMatchCamera":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/rotatesToMatchCamera","abstract":[{"type":"text","text":"A Boolean value that determines whether the annotation view rotates together"},{"type":"text","text":" "},{"type":"text","text":"with the map."}],"type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/rotatestomatchcamera","role":"symbol","title":"rotatesToMatchCamera","fragments":[{"text":"rotatesToMatchCamera","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationImage":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationimage","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationImage","type":"reference"},{"type":"text","text":" class is responsible for presenting point-based"},{"type":"text","text":" "},{"type":"text","text":"annotations visually on a map view. Annotation image objects wrap "},{"code":"UIImage","type":"codeVoice"},{"text":" ","type":"text"},{"text":"objects and may be recycled later and put into a reuse queue that is maintained","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"by the map view."}],"type":"topic","role":"symbol","title":"MLNAnnotationImage","fragments":[{"text":"MLNAnnotationImage","kind":"identifier"}]}}}