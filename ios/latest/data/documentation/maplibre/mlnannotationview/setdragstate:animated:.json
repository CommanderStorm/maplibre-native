{"schemaVersion":{"patch":0,"minor":3,"major":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"tokens":[{"text":"- (","kind":"text"},{"text":"void","kind":"typeIdentifier","preciseIdentifier":"c:v"},{"kind":"text","text":") "},{"kind":"identifier","text":"setDragState:"},{"text":"(","kind":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationViewDragState","text":"MLNAnnotationViewDragState","kind":"typeIdentifier","preciseIdentifier":"c:@E@MLNAnnotationViewDragState"},{"text":") ","kind":"text"},{"kind":"internalParam","text":"dragState"},{"kind":"text","text":" "},{"kind":"identifier","text":"animated:"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:@T@BOOL","kind":"typeIdentifier","text":"BOOL"},{"text":") ","kind":"text"},{"text":"animated","kind":"internalParam"},{"kind":"text","text":";"}],"platforms":["iOS"]}]},{"content":[{"type":"heading","anchor":"discussion","text":"Discussion","level":2},{"inlineContent":[{"type":"text","text":"You can override this method to animate a custom annotation view as the user"},{"text":" ","type":"text"},{"text":"drags it. As the system detects user actions that would indicate a drag, it","type":"text"},{"text":" ","type":"text"},{"text":"calls this method to update the drag state.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/setdragstate:animated:"],"traits":[{"interfaceLanguage":"occ"}]}],"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setDragState:animated:","interfaceLanguage":"occ"},"metadata":{"fragments":[{"kind":"text","text":"- "},{"kind":"identifier","text":"setDragState:animated:"}],"externalID":"c:objc(cs)MLNAnnotationView(im)setDragState:animated:","navigatorTitle":[{"kind":"identifier","text":"setDragState:animated:"}],"remoteSource":{"fileName":"MLNAnnotationView.h","url":"htt