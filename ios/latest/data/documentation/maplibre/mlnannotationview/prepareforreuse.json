{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"abstract":[{"type":"text","text":"Called when the view is removed from the reuse queue."}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/prepareforreuse"]}],"sections":[],"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/prepareForReuse","interfaceLanguage":"occ"},"metadata":{"title":"prepareForReuse","externalID":"c:objc(cs)MLNAnnotationView(im)prepareForReuse","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L117"},"roleHeading":"Instance Method","modules":[{"name":"MapLibre"}],"symbolKind":"method","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"prepareForReuse"}],"navigatorTitle":[{"kind":"identifier","text":"prepareForReuse"}]},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"kind":"identifier","text":"prepareForReuse"},{"kind":"text","text":";"}],"languages":["occ"]}],"kind":"declarations"},{"content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"The default implementation of this method does nothing. You can override it in"},{"text":" ","type":"text"},{"type":"text","text":"your custom annotation view implementation to put the view in a known state"},{"type":"text","text":" "},{"type":"text","text":"before it is returned to your map view delegate."}],"type":"paragraph"}],"kind":"content"}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/prepareForRe