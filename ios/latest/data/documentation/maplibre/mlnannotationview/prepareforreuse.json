{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"primaryContentSections":[{"declarations":[{"tokens":[{"kind":"text","text":"- ("},{"preciseIdentifier":"c:v","kind":"typeIdentifier","text":"void"},{"text":") ","kind":"text"},{"text":"prepareForReuse","kind":"identifier"},{"text":";","kind":"text"}],"platforms":["iOS"],"languages":["occ"]}],"kind":"declarations"},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"inlineContent":[{"text":"The default implementation of this method does nothing. You can override it in","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"your custom annotation view implementation to put the view in a known state"},{"type":"text","text":" "},{"text":"before it is returned to your map view delegate.","type":"text"}],"type":"paragraph"}]}],"kind":"symbol","abstract":[{"type":"text","text":"Called when the view is removed from the reuse queue."}],"metadata":{"fragments":[{"text":"- ","kind":"text"},{"text":"prepareForReuse","kind":"identifier"}],"title":"prepareForReuse","externalID":"c:objc(cs)MLNAnnotationView(im)prepareForReuse","role":"symbol","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L117"},"symbolKind":"method","modules":[{"name":"MapLibre"}],"roleHeading":"Instance Method","navigatorTitle":[{"kind":"identifier","text":"prepareForReuse"}]},"schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/prepareForReuse"},"sections":[],"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/prepareforreuse"],"traits":[{"interfaceLanguage":"occ"}]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"title":"MapLibre","kind":"s