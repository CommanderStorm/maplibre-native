r":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"topic","kind":"symbol","title":"MLNAnnotationView","url":"\/documentation\/maplibre\/mlnannotationview","role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"text":" ","type":"text"},{"text":"object that corresponds to the ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol. When an annotation’s"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/prepareForReuse":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/prepareForReuse","type":"topic","kind":"symbol","title":"prepareForReuse","url":"\/documentation\/maplibre\/mlnannotationview\/prepareforreuse","role":"symbol","fragments":[{"text":"- ","kind":"text"},{"kind":"identifier","text":"prepareForReuse"}],"navigatorTitle":[{"text":"prepareForReuse","kind":"identifier"}],"abstract":[{"type":"text","text":"Called when the view is removed from the reuse queue."}]},"doc://org.swift.MyProject/documentation/MapLibre":{"abstract":[{"type":"text