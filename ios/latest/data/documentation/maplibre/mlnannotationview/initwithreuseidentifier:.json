hat"},{"text":" ","type":"text"},{"text":"annotation by passing the appropriate reuse identifier string to the","type":"text"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/dequeueReusableAnnotationViewWithIdentifier:","isActive":true},{"text":" method.","type":"text"}]}]}],"variants":[{"paths":["\/documentation\/maplibre\/mlnannotationview\/initwithreuseidentifier:"],"traits":[{"interfaceLanguage":"occ"}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"sections":[],"schemaVersion":{"patch":0,"minor":3,"major":0},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/initWithReuseIdentifier:"},"kind":"symbol","abstract":[{"text":"Initializes and returns a new annotation view object.","type":"text"}],"metadata":{"remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L77"},"title":"initWithReuseIdentifier:","externalID":"c:objc(cs)MLNAnnotationView(im)initWithReuseIdentifier:","modules":[{"name":"MapLibre"}],"fragments":[{"kind":"text","text":"- "},{"text":"initWithReuseIdentifier:","kind":"identifier"}],"symbolKind":"method","role":"symbol","navigatorTitle":[{"text":"initWithReuseIdentifier:","kind":"identifier"}],"roleHeading":"Instance Method"},"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/dequeueReusableAnnotationViewWithIdentifier:":{"kind":"symbol","abstract":[{"text":"Returns a reusable annotation view object associated with its identifier.","type":"text"}],"title":"dequeueReusableAnnotationViewWithIdentifier:","url":"\/documentation\/maplibre\/mlnmapview\/dequeuereusableannotationviewwithidentifier:","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/d