ed-examples"},{"items":[{"content":[{"inlineContent":[{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample"}],"type":"paragraph"}]}],"type":"unorderedList"}],"kind":"content"}],"sections":[],"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"schemaVersion":{"patch":0,"major":0,"minor":3},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/setselected:animated:"]}],"metadata":{"navigatorTitle":[{"kind":"identifier","text":"setSelected:animated:"}],"symbolKind":"method","externalID":"c:objc(cs)MLNAnnotationView(im)setSelected:animated:","roleHeading":"Instance Method","remoteSource":{"fileName":"MLNAnnotationView.h","url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L230"},"role":"symbol","title":"setSelected:animated:","modules":[{"name":"MapLibre"}],"fragments":[{"kind":"text","text":"- "},{"text":"setSelected:animated:","kind":"identifier"}]},"abstract":[{"type":"text","text":"Sets the selection state of the annotation view with an optional animation."}],"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","title":"MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true,"type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"text":" ","type":"text"},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"text":" protocol. When an annotation’s","type":"text"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"type":"text","text":" "},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"text":" ","type":"text"},{"type":"text","text":"offscreen."}],"url":"\/documentation\/maplibre\/mlnannotationview","type":"topic","role":"symbol","kind":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","kind":"symbol","abstract":[{"type":"text","text":"The "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true,"type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"type":"text","text":" "},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"type":"text","text":"objects to handle the display."}],"type":"topic","role":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","kind":"symbol","url":"\/documentation\/maplibre","type":"topic","role":"collection"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/setSelected:animated:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/setSelected:animated:","navigatorTitle":[{"text":"setSelected:animated:","kind":"identifier"}],"title":"setSelected:animated:","abstract":[{"text":"Sets the selection state of the annotation view with an optional animation.","type":"text"}],"url":"\/documentation\/maplibre\/mlnannotationview\/setselected:animated:","type":"topic","role":"symbol","kind":"symbol","fragments":[{"kind":"text","text":"- "},{"text":"setSelected:animated:","kind":"identifier"}]},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnnotationViewExample":{"url":"\/documentation\/maplibre-native-for-ios\/annotationviewexample","title":"Custom Annotation View","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/AnnotationViewExample","role":"article","abstract":[{"type":"text","text":"Add a custom annotation view"}],"kind":"article"}}}