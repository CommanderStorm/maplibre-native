bstract":[{"text":"The annotation object currently associated with the view.","type":"text"}],"sections":[],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"url":"\/documentation\/maplibre\/mlnannotation","kind":"symbol","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","title":"MLNAnnotation","fragments":[{"text":"MLNAnnotation","kind":"identifier"}],"abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"type":"text","text":" "},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/annotation":{"kind":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/annotation","type":"topic","url":"\/documentation\/maplibre\/mlnannotationview\/annotation","title":"annotation","role":"symbol","fragments":[{"kind":"identifier","text":"annotation"}],"abstract":[{"text":"The annotation object currently associated with the view.","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"kind":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","role":"symbol","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","title":"MLNAnnotationView","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"abstract":[{"text":"The ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference","isActive":true},{"text":" class is responsible for marking a point annotation","type":"text"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"text":" ","type":"text"},{"type":"text","text":"object that corresponds to the "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference","isActive":true},{"type":"text","text":" protocol. When an annotation’s"},{"text":" ","type":"text"},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"text":"delegate to a corresponding annotation view. If an annotation view is created","type":"text"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"offscreen."}],"type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","url":"\/documentation\/maplibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}],"title":"MapLibre","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic"}}}