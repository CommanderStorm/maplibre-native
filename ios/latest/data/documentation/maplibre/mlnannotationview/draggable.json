lineContent":[{"text":"Setting this property to ","type":"text"},{"type":"codeVoice","code":"YES"},{"type":"text","text":" lets the map view know that the annotation is"},{"text":" ","type":"text"},{"text":"always draggable. In other words, you cannot conditionalize drag operations by","type":"text"},{"type":"text","text":" "},{"type":"text","text":"attempting to stop an operation that has already been initiated; doing so can"},{"type":"text","text":" "},{"text":"lead to undefined behavior. Once begun, the drag operation should always","type":"text"},{"type":"text","text":" "},{"text":"continue to completion.","type":"text"}]},{"type":"heading","text":"Related examples","anchor":"Related-examples","level":4},{"type":"paragraph","inlineContent":[{"type":"text","text":"TODO: Enable users to drag "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","isActive":true},{"text":" objects on your map.","type":"text"}]}]}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/draggable"]}],"abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is draggable."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"title":"MLNAnnotationView","type":"topic","fragments":[{"kind":"identifier","text":"MLNAnnotationView"}],"kind":"symbol","role":"symbol","url":"\/documentation\/maplibre\/mlnannotationview","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},{"type":"text","text":" class is responsible for marking a point annotation"},{"type":"text","text":" "},{"type":"text","text":"with a view. Annotation views represent an annotation object, which is an"},{"type":"text","text":" "},{"text":"object that corresponds to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol. When an annotation’s","type":"text"},{"text":" ","type":"text"},{"text":"geographic coordinate is visible in the map view, the map view asks its","type":"text"},{"type":"text","text":" "},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"type":"text","text":" "},{"text":"with a reuse identifier, the map view may recycle the view when it goes","type":"text"},{"text":" ","type":"text"},{"text":"offscreen.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnannotation","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","abstract":[{"text":"The ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"},{"text":" protocol is used to provide annotation-related information","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"type":"text","text":" "},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the visual representation of the annotation but typically coordinate (in"},{"type":"text","text":" "},{"type":"text","t