type":"heading","level":4,"anchor":"Related-examples","text":"Related examples"},{"inlineContent":[{"text":"TODO: Enable users to drag ","type":"text"},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"type":"text","text":" objects on your map."}],"type":"paragraph"}],"kind":"content"}],"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView"]]},"sections":[],"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","interfaceLanguage":"occ"},"kind":"symbol","abstract":[{"type":"text","text":"A Boolean value indicating whether the annotation view is draggable."}],"metadata":{"remoteSource":{"url":"https:\/\/github.com\/maplibre\/maplibre-native\/blob\/main\/platform\/ios\/src\/MLNAnnotationView.h#L261","fileName":"MLNAnnotationView.h"},"role":"symbol","roleHeading":"Instance Property","fragments":[{"text":"draggable","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"draggable"}],"modules":[{"name":"MapLibre"}],"title":"draggable","symbolKind":"property","externalID":"c:objc(cs)MLNAnnotationView(py)draggable"},"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotationview\/draggable"]}],"references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView","type":"reference"},{"text":" class is responsible for marking a point annotation","type":"text"},{"type":"text","text":" "},{"text":"with a view. Annotation views represent an annotation object, which is an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"object that corresponds to the "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"text":" protocol. When an annotation’s","type":"text"},{"type":"text","text":" "},{"type":"text","text":"geographic coordinate is visible in the map view, the map view asks its"},{"text":" ","type":"text"},{"type":"text","text":"delegate to a corresponding annotation view. If an annotation view is created"},{"text":" ","type":"text"},{"type":"text","text":"with a reuse identifier, the map view may recycle the view when it goes"},{"type":"text","text":" "},{"type":"text","text":"offscreen."}],"url":"\/documentation\/maplibre\/mlnannotationview","role":"symbol","fragments":[{"text":"MLNAnnotationView","kind":"identifier"}],"type":"topic","kind":"symbol","title":"MLNAnnotationView"},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation":{"kind":"symbol","role":"symbol","type":"topic","abstract":[{"type":"text","text":"The "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","isActive":true},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"text":"to a map view. To use this protocol, you adopt it in any custom objects that","type":"text"},{"type":"text","text":" "},{"text":"store or represent annotation data. Each object then serves as the source of","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"information about a single map annotation and provides critical information,"},{"text":" ","type":"text"},{"text":"such as the annotation’s location on the map. Annotation objects do not provide","type":"text"},{"type":"text","text":" "},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"text":"conjunction with the map view’s delegate) the creation of an appropriate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"objects to handle the display."}],"title":"MLNAnnotation","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","url":"\/documentation\/maplibre\/mlnannotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}]},"doc://org.swift.MyProject/documentation/MapLibre":{"kind":"symbol","url":"\/documentation\/maplibre","role":"collection","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre","type":"topic","title":"MapLibre","abstract":[{"type":"text","text":"Powerful, free and open-source mapping toolkit with full control over data sources and styling."}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotationView/draggable":{"role":"symbol","fragments":[{"kind":"identifier","text":"draggable"}],"abstract":[{"text":"A Boolean value indicating whether the annotation view is draggable.","type":"text"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotationView\/draggable","url":"\/documentation\/maplibre\/mlnannotationview\/draggable","title":"draggable","type":"topic","kind":"symbol"}}}