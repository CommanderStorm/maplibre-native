{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource"]]},"abstract":[{"text":"Returns an array of map features loaded by this source, restricted to the given","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"source layers and filtered by the given predicate."}],"variants":[{"paths":["\/documentation\/maplibre\/mlnvectortilesource\/featuresinsourcelayerswithidentifiers:predicate:"],"traits":[{"interfaceLanguage":"occ"}]}],"sections":[],"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"tokens":[{"text":"- (","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:Q$objc(cs)NSArray","text":"NSArray<id<MLNFeature>>"},{"text":" *) ","kind":"text"},{"text":"featuresInSourceLayersWithIdentifiers:","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"NSSet<NSString *>","preciseIdentifier":"c:Q$objc(cs)NSSet"},{"text":" *) ","kind":"text"},{"text":"sourceLayerIdentifiers","kind":"internalParam"},{"text":" ","kind":"text"},{"text":"predicate:","kind":"identifier"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","text":"NSPredicate","preciseIdentifier":"c:objc(cs)NSPredicate"},{"text":" *) ","kind":"text"},{"kind":"internalParam","text":"predicate"},{"text":";","kind":"text"}],"languages":["occ"]}]},{"parameters":[{"content":[{"inlineContent":[{"text":"The source layers to include in the query. Only","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"the features contained in these source layers are included in the returned"},{"text":" ","type":"text"},{"text":"array. This array may not be empty.","type":"text"}],"type":"paragraph"}],"name":"sourceLayerIdentifiers"},{"name":"predicate","content":[{"inlineContent":[{"text":"A predicate to filter the returned features. Use ","type":"text"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" to"},{"text":" ","type":"text