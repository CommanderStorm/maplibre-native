":"symbol","abstract":[{"text":"An ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature"},{"type":"text","text":" object associates a point collection with an"},{"text":" ","type":"text"},{"type":"text","text":"optional identifier and attributes."}],"title":"MLNPointCollectionFeature","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollectionFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMultiPolyline":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline","url":"\/documentation\/maplibre\/mlnmultipolyline","fragments":[{"text":"MLNMultiPolyline","kind":"identifier"}],"role":"symbol","abstract":[{"type":"text","text":"An "},{"type":"reference","isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMultiPolyline"},{"type":"text","text":" object represents a shape consisting of one or more"},{"type":"text","text":" "},{"text":"polylines. For example, you could use a multipolyline shape to represent both","type":"text"},{"type":"text","text":" "},{"type":"text","text":"sides of a divided highway (dual carriageway), excluding the median (central"},{"type":"text","text":" "},{"type":"text","text":"reservation): each carriageway would be a distinct "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"type":"text","text":" object."}],"title":"MLNMultiPolyline","type":"topic","kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"fragments":[{"kind":"identifier","text":"MLNVectorTileSource"}],"title":"MLNVectorTileSource","role":"symbol","type":"topic","url":"\/documentation\/maplibre\/mlnvectortilesource","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"type":"text","