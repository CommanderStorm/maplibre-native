{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"variants":[{"paths":["\/documentation\/maplibre\/mlnannotation\/title"],"traits":[{"interfaceLanguage":"occ"}]}],"primaryContentSections":[{"declarations":[{"languages":["occ"],"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"@property"},{"text":" (","kind":"text"},{"kind":"keyword","text":"nonatomic"},{"text":", ","kind":"text"},{"text":"copy","kind":"keyword"},{"text":", ","kind":"text"},{"kind":"keyword","text":"readonly"},{"text":") ","kind":"text"},{"text":"NSString","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString"},{"text":" * ","kind":"text"},{"text":"title","kind":"identifier"},{"text":";","kind":"text"}]}],"kind":"declarations"},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Although this property is optional, if you support the selection of annotations"},{"type":"text","text":" "},{"type":"text","text":"in your map view, you are expected to provide this property. This string is"},{"type":"text","text":" "},{"type":"text","text":"displayed in the callout for the associated annotation."}]}],"kind":"content"}],"sections":[],"metadata":{"title":"title","externalID":"c:objc(pl)MLNAnnotation(py)title","modules":[{"name":"MapLibre"}],"role":"symbol","symbolKind":"property","navigatorTitle":[{"text":"title","kind":"identifier"}],"fragments":[{"kind":"identifier","text":"title"}],"roleHeading":"Instance Property"},"abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","interfaceLanguage":"occ"},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"role":"symbol","type":"topic","k