{"abstract":[{"text":"The string containing the annotation’s title.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"occ"}],"paths":["\/documentation\/maplibre\/mlnannotation\/title"]}],"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["occ"],"tokens":[{"kind":"keyword","text":"@property"},{"text":" (","kind":"text"},{"text":"nonatomic","kind":"keyword"},{"kind":"text","text":", "},{"kind":"keyword","text":"copy"},{"text":", ","kind":"text"},{"text":"readonly","kind":"keyword"},{"kind":"text","text":") "},{"preciseIdentifier":"c:objc(cs)NSString","kind":"typeIdentifier","text":"NSString"},{"text":" * ","kind":"text"},{"text":"title","kind":"identifier"},{"kind":"text","text":";"}],"platforms":["iOS"]}]},{"content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"inlineContent":[{"text":"Although this property is optional, if you support the selection of annotations","type":"text"},{"type":"text","text":" "},{"text":"in your map view, you are expected to provide this property. This string is","type":"text"},{"type":"text","text":" "},{"type":"text","text":"displayed in the callout for the associated annotation."}],"type":"paragraph"}],"kind":"content"}],"metadata":{"externalID":"c:objc(pl)MLNAnnotation(py)title","title":"title","modules":[{"name":"MapLibre"}],"fragments":[{"kind":"identifier","text":"title"}],"symbolKind":"property","role":"symbol","roleHeading":"Instance Property","navigatorTitle":[{"text":"title","kind":"identifier"}]},"identifier":{"url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title","interfaceLanguage":"occ"},"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"kind":"symbol","references":{"doc://org.swift.MyProject/documentation/MapLibre/MLNAnnotation/title":{"type":"topic","title":"title","i