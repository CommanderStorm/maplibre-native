{"hierarchy":{"paths":[["doc:\/\/org.swift.MyProject\/documentation\/MapLibre","doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation"]]},"primaryContentSections":[{"declarations":[{"languages":["occ"],"tokens":[{"kind":"keyword","text":"@property"},{"kind":"text","text":" ("},{"text":"nonatomic","kind":"keyword"},{"text":", ","kind":"text"},{"kind":"keyword","text":"copy"},{"text":", ","kind":"text"},{"kind":"keyword","text":"readonly"},{"kind":"text","text":") "},{"text":"NSString","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)NSString"},{"kind":"text","text":" * "},{"kind":"identifier","text":"title"},{"kind":"text","text":";"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"content","content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"inlineContent":[{"text":"Although this property is optional, if you support the selection of annotations","type":"text"},{"type":"text","text":" "},{"type":"text","text":"in your map view, you are expected to provide this property. This string is"},{"text":" ","type":"text"},{"type":"text","text":"displayed in the callout for the associated annotation."}],"type":"paragraph"}]}],"sections":[],"kind":"symbol","metadata":{"title":"title","role":"symbol","modules":[{"name":"MapLibre"}],"fragments":[{"text":"title","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"title"}],"roleHeading":"Instance Property","symbolKind":"property","externalID":"c:objc(pl)MLNAnnotation(py)title"},"variants":[{"paths":["\/documentation\/maplibre\/mlnannotation\/title"],"traits":[{"interfaceLanguage":"occ"}]}],"schemaVersion":{"major":0,"patch":0,"minor":3},"identifier":{"interfaceLanguage":"occ","url":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation\/title"},"abstract":[{"type":"text","text":"The string containing the annotation’s title."}],"references":{"doc://org.swift.MyProject/documentation/MapLibre":{"type":"topic","kind":"symbol","abstract":[{"type":"t