tation/MapLibre/MLNPointAnnotation/coordinate":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointAnnotation\/coordinate","url":"\/documentation\/maplibre\/mlnpointannotation\/coordinate","fragments":[{"kind":"identifier","text":"coordinate"}],"role":"symbol","title":"coordinate","type":"topic","abstract":[{"type":"text","text":"The coordinate point of the shape, specified as a latitude and longitude."}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollection":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","url":"\/documentation\/maplibre\/mlnshapecollection","type":"topic","role":"symbol","title":"MLNShapeCollection","abstract":[{"type":"text","text":"An "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollection","type":"reference"},{"text":" object represents a shape consisting of zero or more","type":"text"},{"type":"text","text":" "},{"type":"text","text":"distinct but related shapes that are instances of "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShape","isActive":true,"type":"reference"},{"type":"text","text":". The constituent"},{"text":" ","type":"text"},{"type":"text","text":"shapes can be a mixture of different kinds of shapes."}],"fragments":[{"text":"MLNShapeCollection","kind":"identifier"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPointCollection":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","url":"\/documentation\/maplibre\/mlnpointcollection","type":"topic","role":"symbol","title":"MLNPointCollection","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPointCollection","isActive":true,"type":"reference"},{"type":"text","text":" object represents a shape consisting of one or more"},{"text":" ","type":"text"},{"type":"text","text":"disconnected vertices, specified as "},{"type":"codeVoice","code":"CLLocationCoordinate2D"},{"type":"text","text":" instances. The"},{"text":" ","type":"text"},{"text":"points in the collection may be related but are not connected spatially. For","type":"text"},{"type":"text","text":" "},{"text":"example, you could use a point collection to represent all the trees in an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"orchard."}],"fragments":[{"kind":"identifier","text":"MLNPointCollection"}],"kind":"symbol"},"doc://org.swift.MyProject/documentation/MapLibre/MLNVectorTileSource":{"type":"topic","title":"MLNVectorTileSource","kind":"symbol","role":"symbol","abstract":[{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","isActive":true},{"text":" is a map content source that supplies tiled vector data","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"in "},{"type":"text","text":"Mapbox Vector Tile"},{"text":" format","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to be shown on the map. The location of and metadata about the tiles are"},{"text":" ","type":"text"},{"text":"defined either by an option dictionary or by an external file that conforms to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"the"},{"text":" ","type":"text"},{"type":"text","text":"TileJSON specification"},{"text":".","type":"text"},{"type":"text","text":" "},{"text":"A vector tile source is added to an ","type":"text"},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNStyle","isActive":true,"type":"reference"},{"text":" object along with one or more","type":"text"},{"type":"text","text":" "},{"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorStyleLayer","isActive":true},{"text":" objects. A vector style layer defines the appearance of","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"any content supplied by the vector tile source."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNVectorTileSource","url":"\/documentation\/maplibre\/mlnvectortilesource","fragments":[{"text":"MLNVectorTileSource","kind":"identifier"}]}}}