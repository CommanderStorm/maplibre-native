ifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","fragments":[{"kind":"identifier","text":"MLNAnnotation"}],"type":"topic","abstract":[{"type":"text","text":"The "},{"isActive":true,"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNAnnotation","type":"reference"},{"type":"text","text":" protocol is used to provide annotation-related information"},{"type":"text","text":" "},{"type":"text","text":"to a map view. To use this protocol, you adopt it in any custom objects that"},{"text":" ","type":"text"},{"type":"text","text":"store or represent annotation data. Each object then serves as the source of"},{"text":" ","type":"text"},{"text":"information about a single map annotation and provides critical information,","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"such as the annotation’s location on the map. Annotation objects do not provide"},{"text":" ","type":"text"},{"text":"the visual representation of the annotation but typically coordinate (in","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"conjunction with the map view’s delegate) the creation of an appropriate"},{"type":"text","text":" "},{"text":"objects to handle the display.","type":"text"}],"role":"symbol","url":"\/documentation\/maplibre\/mlnannotation","title":"MLNAnnotation"},"doc://org.swift.MyProject/documentation/MapLibre/MLNPolyline":{"abstract":[{"type":"text","text":"An "},{"isActive":true,"type":"reference","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline"},{"text":" object represents a shape consisting of two or more vertices,","type":"text"},{"type":"text","text":" "},{"text":"specified as ","type":"text"},{"code":"CLLocationCoordinate2D","type":"codeVoice"},{"type":"text","text":" instances, and the line segments that"},{"text":" ","type":"text"},{"type":"text","text":"connect them. For example, you could use an polyline to represent a road or the"},{"type":"text","text":" "},{"type":"text","text":"path along which something moves."}],"role":"symbol","kind":"symbol","url":"\/documentation\/maplibre\/mlnpolyline","title":"MLNPolyline","fragments":[{"kind":"identifier","text":"MLNPolyline"}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNPolyline","type":"topic"},"doc://org.swift.MyProject/documentation/MapLibre/MLNShapeCollectionFeature":{"type":"topic","url":"\/documentation\/maplibre\/mlnshapecollectionfeature","abstract":[{"type":"text","text":"An "},{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","isActive":true,"type":"reference"},{"type":"text","text":" object associates a shape collection with an"},{"type":"text","text":" "},{"type":"text","text":"optional identifier and attributes."}],"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNShapeCollectionFeature","kind":"symbol","role":"symbol","fragments":[{"kind":"identifier","text":"MLNShapeCollectionFeature"}],"title":"MLNShapeCollectionFeature"},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapViewDelegate/mapView:viewForAnnotation:":{"url":"\/documentation\/maplibre\/mlnmapviewdelegate\/mapview:viewforannotation:","title":"mapView:viewForAnnotation:","type":"topic","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapViewDelegate\/mapView:viewForAnnotation:","role":"symbol","abstract":[{"text":"Returns a view object to mark the given point annotation object on the map.","type":"text"}],"fragments":[{"kind":"text","text":"- "},{"text":"mapView:viewForAnnotation:","kind":"identifier"}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"mapView:viewForAnnotation:"}]},"doc://org.swift.MyProject/documentation/MapLibre/MLNMapView/addAnnotation:":{"identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre\/MLNMapView\/addAnnotation:","title":"addAnnotation:","navigatorTitle":[{"kind":"identifier","text":"addAnnotation:"}],"kind":"symbol","type":"topic","role":"symbol","url":"\/documentation\/maplibre\/mlnmapview\/addannotation:","fragments":[{"kind":"text","text":"- "},{"text":"addAnnotation:","kind":"identifier"}],"abstract":[{"type":"text","text":"Adds an annotation to the map view."}]}}}