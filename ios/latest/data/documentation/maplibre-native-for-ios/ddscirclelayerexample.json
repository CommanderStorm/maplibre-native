te = NSPredicate(format: \"class == %@\", \"shop\")","","        \/\/ Style the circle layer color based on the rank","        layer.circleColor = NSExpression(mglJSONObject: [\"step\", [\"get\", \"rank\"], 0, \"red\", 10, \"green\", 20, \"blue\", 30, \"purple\", 40, \"yellow\"] as [Any])","        layer.circleRadius = NSExpression(forConstantValue: 3)","","        style.addLayer(layer)","    }","}"]},{"inlineContent":[{"identifier":"DDSCircleLayerExample.png","type":"image"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Map data from OpenStreetMap. Style uses OpenMapTiles.","type":"text"}]}]}],"references":{"https://openmaptiles.org/schema/#poi":{"title":"POIs","url":"https:\/\/openmaptiles.org\/schema\/#poi","type":"link","identifier":"https:\/\/openmaptiles.org\/schema\/#poi","titleInlineContent":[{"text":"POIs","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/GeoJSON":{"role":"article","abstract":[],"kind":"article","type":"topic","title":"Working with GeoJSON Data","url":"\/documentation\/maplibre-native-for-ios\/geojson","identifier":"doc:\/\/org.swift.MyProject\/documentation\/MapLibre-Native-for-iOS\/GeoJSON"},"DDSCircleLayerExample.png":{"variants":[{"url":"\/images\/DDSCircleLayerExample@2x.png","traits":["2x","light"]}],"type":"image","alt":null,"identifier":"DDSCircleLayerExample.png"},"https://github.com/maplibre/demotiles/tree/gh-pages/tiles-omt":{"title":"tiles around Innsbruck, Austria","url":"https:\/\/github.com\/maplibre\/demotiles\/tree\/gh-pages\/tiles-omt","type":"link","identifier":"https:\/\/github.com\/maplibre\/demotiles\/tree\/gh-pages\/tiles-omt","titleInlineContent":[{"text":"tiles around Innsbruck, Austria","type":"text"}]},"doc://org.swift.MyProject/documentation/MapLibre-Native-for-iOS/AnimatedLineExample":{"url":"\/documentation\/maplibre-native-for-ios\/animatedlineexample","type":"topic","title":"Animated Line","kind":"article","identifier":"doc:\/\/org.swift.MyProject\/document